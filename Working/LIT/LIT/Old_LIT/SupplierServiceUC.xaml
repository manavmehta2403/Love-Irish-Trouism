<Window x:Class="LIT.Old_LIT.SupplierServiceUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:behavior="clr-namespace:LIT.Core.Behaviors;assembly=LIT.Core"
             xmlns:local="clr-namespace:LIT"
             mc:Ignorable="d" 
             d:DesignHeight="850" d:DesignWidth="800" WindowStartupLocation="CenterScreen" WindowState="Maximized" >
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LIT.Core;component/Styles/PasswordBoxStyles.xaml"/>
                <ResourceDictionary Source="/LIT.Core;component/Styles/TextBoxStyles.xaml"/>
                <ResourceDictionary Source="/LIT.Core;component/Styles/ButtonStyles.xaml"/>
                <ResourceDictionary Source="/LIT.Core;component/Styles/TabItemStyles.xaml"/>

                <ResourceDictionary Source="/LIT.Core;component/Styles/ScrollBarStyles.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Window.Resources>
    <Grid>
        <StackPanel Grid.ColumnSpan="2">
            <Label x:Name="lblserviceNameUC" Visibility="Hidden" HorizontalAlignment="Right" FontWeight="Medium" FontSize="13" Margin="0 0 10 2"></Label>
            <Label x:Name="lblRatesDateUC" Visibility="Hidden" HorizontalAlignment="Right" FontWeight="Medium" FontSize="13" Margin="0 -10 10 0"></Label>
        </StackPanel>
        <TabControl Width="auto" Grid.Row="0" Height="auto" MinWidth="{Binding ElementName=Navigationframe, Path=ActualWidth}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" Background="white" BorderBrush="#FF4D4D4D" BorderThickness="1,1,1,1" >


            <TabItem Header="Services" Name="SupplierServices" Style="{StaticResource TabItemWhite}" Width="131" FontSize="16" Height="48" Cursor="Hand" Margin="-4,0,0,0" >


                <DockPanel>
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="auto"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*"></RowDefinition>
                        </Grid.RowDefinitions>

                        <StackPanel Grid.Column="0" Grid.Row="0"  x:Name="StackServicesUC" DockPanel.Dock="Left"  Background="#f9f9f9" VerticalAlignment="Stretch" >
                            <StackPanel VerticalAlignment="Center" >
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                    </Grid.ColumnDefinitions>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="30"/>
                                    </Grid.RowDefinitions>
                                    <Label  FontSize="14" Content="Services" 
                                        FontWeight="Medium" Foreground="#25304C" Height="30" Width="auto"  Grid.Column="0" Grid.Row="0"/>

                                    <Button  Style="{StaticResource btnBlue}" x:Name="BtnServiceAddUc" Click="BtnServiceAddUc_Click" 
                                          Background="Transparent" VerticalAlignment="Center"  
                                         VerticalContentAlignment="Center" FontFamily="Calibri" Height="30" FontSize="16"
                                         BorderBrush="#FF579F00" Foreground="#FF579F00" FontWeight="Bold" MaxWidth="140"
                                         Width="58" Cursor="Hand" HorizontalAlignment="Right"  Grid.Column="1" Grid.Row="0" >

                                        <StackPanel Orientation="Horizontal">
                                            <Image Source="/LIT.Core;component/Media/Images/plus.png" Width="13" VerticalAlignment="Center"></Image>
                                            <Label Foreground="#FF579F00" FontSize="16" VerticalAlignment="Center">Add</Label>
                                        </StackPanel>

                                    </Button>
                                </Grid>
                               
                            </StackPanel>
                            <StackPanel Orientation="Vertical" HorizontalAlignment="Stretch" Margin="0 " >
                                <ScrollViewer VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Visible">

                                    <DataGrid behavior:NewSingleClickDataGridBehavior.EnableSingleClickEditing="True" x:Name="dgSupplierServicesuc" ColumnWidth="*"  MinHeight="315" MaxHeight="450" VerticalAlignment="Stretch" BorderThickness="1,1,1,0" BorderBrush="#f9f9f9"  AutoGenerateColumns="False" CanUserAddRows="False" HorizontalGridLinesBrush="#C9C9C9" HorizontalAlignment="Left" VerticalGridLinesBrush="#C9C9C9" Background="#f9f9f9" VerticalScrollBarVisibility="Auto">
                                    <DataGrid.Resources>
                                        <Style TargetType="{x:Type DataGridCell}">
                                            <EventSetter Event="MouseDoubleClick" Handler="dgSupplierServicesucDataGridCell_MouseDoubleClick"/>
                                        </Style>
                                    </DataGrid.Resources>
                                    <DataGrid.Columns>

                                        <DataGridTextColumn Header="ServiceID" Binding="{Binding ServiceID}"  Visibility="Hidden" Width="0">
                                            <DataGridTextColumn.HeaderStyle>
                                                <Style TargetType="DataGridColumnHeader">
                                                    <Setter Property="HorizontalContentAlignment" Value="Center" />
                                                </Style>
                                            </DataGridTextColumn.HeaderStyle>

                                        </DataGridTextColumn>
                                        <DataGridTextColumn Header="SupplierID" Binding="{Binding SupplierID}" Visibility="Hidden" Width="0">
                                            <DataGridTextColumn.CellStyle>
                                                <Style>
                                                    <!--<Setter Property="ToolTip" Value="{Binding SupplierID}">
                                                    
                                                </Setter>-->
                                                </Style>
                                            </DataGridTextColumn.CellStyle>
                                        </DataGridTextColumn>
                                        <DataGridCheckBoxColumn Header="Active" x:Name="chbsupplieractiveuc" Binding="{Binding IsActive, UpdateSourceTrigger=PropertyChanged}" Visibility="Hidden" MinWidth="50">
                                            <DataGridCheckBoxColumn.HeaderStyle>
                                                <Style TargetType="DataGridColumnHeader">
                                                    <Setter Property="HorizontalContentAlignment" Value="Center" />
                                                    <Setter Property="Foreground" Value="#252E39" />
                                                    <Setter Property="FontSize" Value="14" />
                                                    <Setter Property="FontWeight" Value="Medium" />
                                                    <Setter Property="FontFamily" Value="Calibri" />
                                                </Style>
                                            </DataGridCheckBoxColumn.HeaderStyle>
                                            <DataGridCheckBoxColumn.ElementStyle >
                                                <Style TargetType="CheckBox">
                                                    <Setter Property="IsChecked" Value="{Binding IsActive}"></Setter>
                                                    <Setter Property="HorizontalAlignment" Value="Center" />
                                                    <Setter Property="HorizontalContentAlignment" Value="Center" />
                                                    <Setter Property="VerticalAlignment" Value="Center" />
                                                    <Setter Property="FontFamily" Value="Calibri" />
                                                        <Setter Property="FontSize" Value="14" />
                                                        <Setter Property="IsEnabled" Value="False"></Setter>
                                                        <!--<Setter Property="behavior:SingleClickCheckBoxBehavior.IsEnabled" Value="True"></Setter>-->
                                                    </Style>
                                            </DataGridCheckBoxColumn.ElementStyle>
                                            <DataGridCheckBoxColumn.EditingElementStyle>
                                                <Style TargetType="CheckBox">
                                                    <Setter Property="IsChecked" Value="{Binding IsActive, Mode=OneWay}"/>
                                                    <Setter Property="HorizontalAlignment" Value="Center" />
                                                    <Setter Property="HorizontalContentAlignment" Value="Center" />
                                                    <Setter Property="VerticalAlignment" Value="Center" />
                                                    <Setter Property="FontFamily" Value="Calibri" />
                                                    <Setter Property="IsEnabled" Value="False"></Setter>
                                                </Style>
                                            </DataGridCheckBoxColumn.EditingElementStyle>

                                        </DataGridCheckBoxColumn>
                                        <DataGridTemplateColumn Header="Service Name" MinWidth="212" Width="*">
                                            <DataGridTemplateColumn.HeaderStyle>
                                                <Style TargetType="DataGridColumnHeader">
                                                    <Setter Property="HorizontalContentAlignment" Value="Center" />
                                                    <Setter Property="HorizontalAlignment" Value="Stretch" />
                                                    <Setter Property="VerticalAlignment" Value="Stretch" />
                                                    <Setter Property="Foreground" Value="#252E39" />
                                                    <Setter Property="FontSize" Value="14" />
                                                    <Setter Property="FontWeight" Value="Medium" />
                                                    <Setter Property="FontFamily" Value="Calibri" />

                                                </Style>
                                            </DataGridTemplateColumn.HeaderStyle>
                                            <DataGridTemplateColumn.CellTemplate>
                                                <DataTemplate>
                                                    <TextBlock Grid.Column="0" Text="{Binding ServiceName, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" VerticalAlignment="Center" FontFamily="Calibri" FontSize="14" Padding="5 0 0 0">
                                                    </TextBlock>
                                                </DataTemplate>
                                            </DataGridTemplateColumn.CellTemplate>
                                            <DataGridTemplateColumn.CellEditingTemplate>
                                                <DataTemplate>
                                                    <Grid FocusManager.FocusedElement="{Binding ElementName= txtServicename}">
                                                        <TextBox Grid.Column="0" x:Name="txtServicename" Text="{Binding ServiceName, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" VerticalAlignment="Center" FontFamily="Calibri" FontSize="14" Padding="5 0 0 0">
                                                        </TextBox>
                                                    </Grid>
                                                </DataTemplate>
                                            </DataGridTemplateColumn.CellEditingTemplate>
                                        </DataGridTemplateColumn>


                                        <DataGridComboBoxColumn Header="Type" x:Name="CmbSupplierServiceTypeUC"  DisplayMemberPath="."
                                             SelectedItemBinding="{Binding SelectedItem, UpdateSourceTrigger=PropertyChanged}" MinWidth="180" Width="*"  >
                                            <DataGridComboBoxColumn.HeaderStyle>
                                                <Style TargetType="DataGridColumnHeader">
                                                    <Setter Property="HorizontalContentAlignment" Value="Center" />
                                                    <Setter Property="Foreground" Value="#252E39" />
                                                    <Setter Property="FontSize" Value="14" />
                                                    <Setter Property="FontWeight" Value="Medium" />
                                                    <Setter Property="FontFamily" Value="Calibri" />
                                                </Style>
                                            </DataGridComboBoxColumn.HeaderStyle>
                                            <DataGridComboBoxColumn.ElementStyle >
                                                <Style TargetType="ComboBox">
                                                    <Setter Property="ItemsSource" Value="{Binding Path=ListofSupplierServiceType, Mode=OneWay}" />
                                                    <Setter Property="IsSynchronizedWithCurrentItem" Value="False"/>
                                                    <Setter Property="HorizontalAlignment" Value="Center" />
                                                    <Setter Property="HorizontalContentAlignment" Value="Left" />
                                                    <Setter Property="VerticalAlignment" Value="Center" />
                                                    <Setter Property="FontFamily" Value="Calibri" />
                                                    <Setter Property="FontSize" Value="14" />
                                                </Style>
                                            </DataGridComboBoxColumn.ElementStyle>
                                            <DataGridComboBoxColumn.EditingElementStyle>
                                                <Style TargetType="ComboBox">
                                                    <Setter Property="ItemsSource" Value="{Binding Path=ListofSupplierServiceType, Mode=OneWay}"/>
                                                    <Setter Property="IsDropDownOpen" Value="True" />
                                                    <Setter Property="HorizontalAlignment" Value="Center" />
                                                    <Setter Property="HorizontalContentAlignment" Value="Left" />
                                                    <Setter Property="VerticalAlignment" Value="Center" />
                                                    <Setter Property="FontFamily" Value="Calibri" />
                                                </Style>
                                            </DataGridComboBoxColumn.EditingElementStyle>

                                        </DataGridComboBoxColumn>


                                        <DataGridComboBoxColumn Header="Currency" x:Name="CmbCurrencyDetailsUC"  DisplayMemberPath="."
                                             SelectedItemBinding="{Binding SelectedItemCurrency, UpdateSourceTrigger=PropertyChanged}" MinWidth="160" Width="*">
                                            <DataGridComboBoxColumn.HeaderStyle>
                                                <Style TargetType="DataGridColumnHeader">
                                                    <Setter Property="HorizontalContentAlignment" Value="Center" />
                                                    <Setter Property="Foreground" Value="#252E39" />
                                                    <Setter Property="FontSize" Value="14" />
                                                    <Setter Property="FontWeight" Value="Medium" />
                                                    <Setter Property="FontFamily" Value="Calibri" />
                                                </Style>
                                            </DataGridComboBoxColumn.HeaderStyle>
                                            <DataGridComboBoxColumn.ElementStyle >
                                                <Style TargetType="ComboBox">
                                                    <Setter Property="ItemsSource" Value="{Binding Path=ListofCurrency, Mode=OneWay}" />
                                                    <Setter Property="IsSynchronizedWithCurrentItem" Value="False"/>
                                                    <Setter Property="HorizontalAlignment" Value="Center" />
                                                    <Setter Property="HorizontalContentAlignment" Value="Left" />
                                                    <Setter Property="VerticalAlignment" Value="Center" />
                                                    <Setter Property="FontFamily" Value="Calibri" />
                                                    <Setter Property="FontSize" Value="14" />
                                                </Style>
                                            </DataGridComboBoxColumn.ElementStyle>
                                            <DataGridComboBoxColumn.EditingElementStyle>
                                                <Style TargetType="ComboBox">
                                                    <Setter Property="ItemsSource" Value="{Binding Path=ListofCurrency, Mode=OneWay}"/>
                                                    <Setter Property="IsDropDownOpen" Value="True" />
                                                    <Setter Property="HorizontalAlignment" Value="Center" />
                                                    <Setter Property="HorizontalContentAlignment" Value="Left" />
                                                    <Setter Property="VerticalAlignment" Value="Center" />
                                                    <Setter Property="FontFamily" Value="Calibri" />
                                                </Style>
                                            </DataGridComboBoxColumn.EditingElementStyle>

                                        </DataGridComboBoxColumn>



                                        <DataGridComboBoxColumn Header="Charges" x:Name="CmbChargesgroupinfoUC"  DisplayMemberPath="."
                                             SelectedItemBinding="{Binding SelectedItemGroupInfo, UpdateSourceTrigger=PropertyChanged}" MinWidth="100" Width="*">
                                            <DataGridComboBoxColumn.HeaderStyle>
                                                <Style TargetType="DataGridColumnHeader">
                                                    <Setter Property="HorizontalContentAlignment" Value="Center" />
                                                    <Setter Property="Foreground" Value="#252E39" />
                                                    <Setter Property="FontSize" Value="14" />
                                                    <Setter Property="FontWeight" Value="Medium" />
                                                    <Setter Property="FontFamily" Value="Calibri" />
                                                </Style>
                                            </DataGridComboBoxColumn.HeaderStyle>
                                            <DataGridComboBoxColumn.ElementStyle >
                                                <Style TargetType="ComboBox">
                                                    <Setter Property="ItemsSource" Value="{Binding Path=ListofGroupinfo, Mode=OneWay}" />
                                                    <Setter Property="IsSynchronizedWithCurrentItem" Value="False"/>
                                                    <Setter Property="HorizontalAlignment" Value="Center" />
                                                    <Setter Property="HorizontalContentAlignment" Value="Left" />
                                                    <Setter Property="VerticalAlignment" Value="Center" />
                                                    <Setter Property="FontFamily" Value="Calibri" />
                                                    <Setter Property="FontSize" Value="14" />
                                                </Style>
                                            </DataGridComboBoxColumn.ElementStyle>
                                            <DataGridComboBoxColumn.EditingElementStyle>
                                                <Style TargetType="ComboBox">
                                                    <Setter Property="ItemsSource" Value="{Binding Path=ListofGroupinfo, Mode=OneWay}"/>
                                                    <Setter Property="IsDropDownOpen" Value="True" />
                                                    <Setter Property="HorizontalAlignment" Value="Center" />
                                                    <Setter Property="HorizontalContentAlignment" Value="Left" />
                                                    <Setter Property="VerticalAlignment" Value="Center" />
                                                    <Setter Property="FontFamily" Value="Calibri" />
                                                </Style>
                                            </DataGridComboBoxColumn.EditingElementStyle>

                                        </DataGridComboBoxColumn>


                                        <DataGridTemplateColumn MinWidth="110"  Width="*" Header="Rates" >
                                            <DataGridTemplateColumn.HeaderStyle>
                                                <Style TargetType="DataGridColumnHeader">
                                                    <Setter Property="HorizontalContentAlignment" Value="Center" />
                                                    <Setter Property="Foreground" Value="#252E39" />
                                                    <Setter Property="FontSize" Value="14" />
                                                    <Setter Property="FontWeight" Value="Medium" />
                                                    <Setter Property="FontFamily" Value="Calibri" />
                                                </Style>
                                            </DataGridTemplateColumn.HeaderStyle>
                                            <DataGridTemplateColumn.CellTemplate>

                                                <DataTemplate>
                                                    <Button Name="btnServiceRates" FontFamily="Calibri" Template="{StaticResource ButtonBaseTemplate}" Content="Rates" Background="#579F00"  Foreground="#ffffff" BorderBrush="Transparent" VerticalAlignment="Center" HorizontalAlignment="Center" Click="btnServiceRates_Click" Cursor="Hand" VerticalContentAlignment="Center" Padding="12,1,12,1" MouseEnter="msEnter" MouseLeave="msLeave" FontSize="14" >
                                                        <!--<Image Source="../Images/edit_black.png" Width="16" Height="16" />-->
                                                    </Button>
                                                </DataTemplate>
                                            </DataGridTemplateColumn.CellTemplate>
                                        </DataGridTemplateColumn>

                                    </DataGrid.Columns>
                                </DataGrid>
                                </ScrollViewer>
                            </StackPanel>
                        </StackPanel>
                        <GridSplitter Grid.Column="1" ResizeBehavior="PreviousAndNext" ResizeDirection="Columns"

              HorizontalAlignment="Stretch"

              VerticalAlignment="Stretch"

              Background="#b9b9b9"

              ShowsPreview="True"

              Width="5" Margin="4" />
                        <DockPanel Grid.Column="2" DockPanel.Dock="Right">
                            <StackPanel DockPanel.Dock="Top">

                                <StackPanel HorizontalAlignment="Right" Orientation="Horizontal" Panel.ZIndex="9" VerticalAlignment="Center" >

                                    <CheckBox  FlowDirection="RightToLeft" x:Name="chbhidenonactive" IsThreeState="False" VerticalContentAlignment="Center" FontFamily="Calibri" Content="Hide Non Active Items" IsChecked="True" Click="chbhidenonactive_Click" FontSize="14" FontWeight="Bold" Foreground="#25304c" Margin="0,5,10,5" VerticalAlignment="Center">
                                        <CheckBox.Resources>
                                            <Style TargetType="{x:Type Path}">
                                                <Setter Property="FlowDirection" Value="LeftToRight"/>
                                            </Style>
                                        </CheckBox.Resources>
                                    </CheckBox>


                                    <CheckBox FlowDirection="RightToLeft" x:Name="chbhideexpiredseasons"  IsThreeState="False" VerticalContentAlignment="Center"  FontFamily="Calibri" Content="Hide Expired Seasons" Click="chbhideexpiredseasons_Click" Margin="0,5,10,5" FontSize="14" Foreground="#25304c" FontWeight="Bold" VerticalAlignment="Center">
                                        <CheckBox.Resources>
                                            <Style TargetType="{x:Type Path}">
                                                <Setter Property="FlowDirection" Value="LeftToRight"/>
                                            </Style>
                                        </CheckBox.Resources>
                                    </CheckBox>

                                </StackPanel>

                                <StackPanel  VerticalAlignment="Top" Height="auto" Margin="0,0,0,10"  Background="#f9f9f9">



                                    <TabControl Padding="5" Background="#EBEBEB" Margin="2">

                                        <TabItem Header="Rates" FontFamily="Calibri" Name="SupplierRatesCore" Style="{StaticResource TabItemGrey}" Width="131" FontSize="16" Height="48" Cursor="Hand" Margin="-4,0,0,0">



                                            <Grid x:Name="StackRates" Width="auto" Background="White">
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition MinWidth="500" />
                                                    <ColumnDefinition Width="*" />
                                                </Grid.ColumnDefinitions>
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="28" />
                                                    <RowDefinition Height="*" />
                                                </Grid.RowDefinitions>
                                                <StackPanel Grid.Column="0" Grid.Row="0" Margin="0,4,0,0">
                                                    <Label Content="Season Dates For Selected Services" FontFamily="Calibri" HorizontalAlignment="Left" FontSize="14"  VerticalAlignment="Center" FontWeight="Medium" Foreground="#25304C" Margin="10,0,0,0"></Label>
                                                </StackPanel>
                                                <StackPanel Grid.Column="1" Grid.Row="0" Margin="0,4,0,0">

                                                    <Button Content="&#43; Add" Style="{StaticResource btnBlue}" Name="BtnRatesAdd" Click="BtnRatesAdd_Click" VerticalAlignment="Center" Margin="0,0,10,3" VerticalContentAlignment="Center" FontFamily="Calibri" Height="25" FontSize="16" BorderBrush="#FF579F00" Foreground="#FF579F00" FontWeight="Bold" MaxWidth="140" Width="50" Cursor="Hand" HorizontalAlignment="Right" >
                                                        <Button.Background>
                                                            <SolidColorBrush Color="#FF579F00" Opacity="0"/>
                                                        </Button.Background>

                                                    </Button>
                                                </StackPanel>
                                                <StackPanel Grid.Column="0" Grid.Row="1" Grid.ColumnSpan="2" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="1,15">

                                                    <DataGrid behavior:NewSingleClickDataGridBehavior.EnableSingleClickEditing="True" x:Name="dgSupplierServicesRatesUC" ColumnWidth="*" AutoGenerateColumns="False" CanUserAddRows="False" HorizontalAlignment="Left" BorderThickness="1,1,1,1" BorderBrush="#c9c9c9" HorizontalGridLinesBrush="#C9C9C9" VerticalGridLinesBrush="#C9C9C9" Background="White"
                                                             SelectedCellsChanged="dgSupplierServicesRatesUC_SelectedCellsChanged" 
                                                              RowDetailsVisibilityMode="VisibleWhenSelected"  MaxHeight="200">
                                                        <DataGrid.Resources>
                                                            <Style TargetType="{x:Type DataGridCell}">
                                                                <EventSetter Event="MouseDoubleClick" Handler="dgSupplierServicesRatesUCDataGridCell_MouseDoubleClick"/>
                                                            </Style>
                                                        </DataGrid.Resources>
                                                        <DataGrid.Columns>
                                                            <DataGridTextColumn Header="ServiceID" Binding="{Binding ServiceID}"  Visibility="Hidden">

                                                            </DataGridTextColumn>
                                                            <DataGridTextColumn Header="SupplierServiceDetailsRateId" Binding="{Binding SupplierServiceDetailsRateId}" Visibility="Hidden">

                                                            </DataGridTextColumn>

                                                            <DataGridTemplateColumn Header="Valid From" MaxWidth="105">
                                                                <DataGridTemplateColumn.HeaderStyle>
                                                                    <Style TargetType="{x:Type DataGridColumnHeader}">
                                                                        <Setter Property="HorizontalContentAlignment" Value="Center" />
                                                                        <Setter Property="Foreground" Value="#252E39" />
                                                                        <Setter Property="FontSize" Value="14" />
                                                                        <Setter Property="FontWeight" Value="Medium" />
                                                                        <Setter Property="FontFamily" Value="Calibri"/>
                                                                    </Style>
                                                                </DataGridTemplateColumn.HeaderStyle>
                                                                <DataGridTemplateColumn.CellTemplate>
                                                                    <DataTemplate>
                                                                        <DatePicker  x:Name="Txtvalidfrom"  MaxWidth="150" SelectedDate="{Binding ValidFrom, UpdateSourceTrigger=PropertyChanged}" FontFamily="Calibri" FontSize="14"  VerticalContentAlignment="Center" HorizontalContentAlignment="Center" BorderBrush="Transparent" BorderThickness="0,0,0,0" Loaded="Txtvalidfrom_Loaded" >
                                                                            <DatePicker.Background>
                                                                                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                                                                    <GradientStop Color="Transparent"/>
                                                                                    <GradientStop Color="Transparent" Offset="1"/>
                                                                                </LinearGradientBrush>
                                                                            </DatePicker.Background>
                                                                            <DatePicker.Resources>
                                                                                <Style TargetType="{x:Type DatePickerTextBox}">
                                                                                    <Setter Property="Text" Value="Select date" />
                                                                                    <Setter Property="VerticalContentAlignment" Value="Center" />
                                                                                    <Setter Property="VerticalAlignment" Value="Center" />
                                                                                    <Setter Property="Focusable" Value="False"/>
                                                                                    <!--<Setter Property="IsReadOnly" Value="True"/>-->
                                                                                    <!--<Setter Property="Background">
                                                                                        <Setter.Value>
                                                                                            <ImageBrush ImageSource="/LIT.Core;component/Media/Images/folder.png" Opacity="0"/>
                                                                                        </Setter.Value>
                                                                                    </Setter>-->
                                                                                </Style>
                                                                            </DatePicker.Resources>
                                                                        </DatePicker>
                                                                    </DataTemplate>
                                                                </DataGridTemplateColumn.CellTemplate>
                                                                <DataGridTemplateColumn.CellEditingTemplate>
                                                                    <DataTemplate>
                                                                        <Grid FocusManager.FocusedElement="{Binding ElementName= Txtvalidfrom}" >
                                                                            <DatePicker  x:Name="Txtvalidfrom"  MaxWidth="150" SelectedDate="{Binding ValidFrom, UpdateSourceTrigger=PropertyChanged}" FontFamily="Calibri"  FontSize="14" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" BorderBrush="Transparent" BorderThickness="0,0,0,0"  >
                                                                                <DatePicker.Background>
                                                                                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                                                                        <GradientStop Color="White"/>
                                                                                        <GradientStop Color="White" Offset="1"/>
                                                                                    </LinearGradientBrush>
                                                                                </DatePicker.Background>
                                                                                <DatePicker.Resources>
                                                                                    <Style TargetType="{x:Type DatePickerTextBox}">
                                                                                        <Setter Property="Text" Value="Select date" />
                                                                                        <Setter Property="Focusable" Value="False"/>
                                                                                        <Setter Property="VerticalAlignment" Value="Center" />
                                                                                        <Setter Property="VerticalContentAlignment" Value="Center" />
                                                                                        <!--<Setter Property="IsReadOnly" Value="True"/>-->
                                                                                        <!--<Setter Property="Background">
                                                                                            <Setter.Value>
                                                                                                <ImageBrush ImageSource="/LIT.Core;component/Media/Images/folder.png" Opacity="0"/>
                                                                                            </Setter.Value>
                                                                                        </Setter>-->

                                                                                    </Style>
                                                                                </DatePicker.Resources>
                                                                            </DatePicker>
                                                                        </Grid>
                                                                    </DataTemplate>
                                                                </DataGridTemplateColumn.CellEditingTemplate>
                                                            </DataGridTemplateColumn>

                                                            <DataGridTemplateColumn Header="Valid To"  MaxWidth="105">
                                                                <DataGridTemplateColumn.HeaderStyle>
                                                                    <Style TargetType="DataGridColumnHeader">
                                                                        <Setter Property="HorizontalContentAlignment" Value="Center" />
                                                                        <!--<Setter Property="HorizontalAlignment" Value="Stretch" />
                                                                                <Setter Property="VerticalAlignment" Value="Stretch" />-->
                                                                        <Setter Property="Foreground" Value="#252E39" />
                                                                        <Setter Property="FontSize" Value="14" />
                                                                        <Setter Property="FontWeight" Value="Medium" />
                                                                        <Setter Property="FontFamily" Value="Calibri"/>
                                                                    </Style>
                                                                </DataGridTemplateColumn.HeaderStyle>
                                                                <DataGridTemplateColumn.CellTemplate>
                                                                    <DataTemplate>
                                                                        <DatePicker  x:Name="Txtvalidto"  SelectedDate="{Binding ValidTo, UpdateSourceTrigger=PropertyChanged}" FontFamily="Calibri" FontSize="14"  VerticalContentAlignment="Center" HorizontalContentAlignment="Center" BorderBrush="Transparent" BorderThickness="0,0,0,0">
                                                                            <DatePicker.Background>
                                                                                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                                                                    <GradientStop Color="Transparent"/>
                                                                                    <GradientStop Color="Transparent" Offset="1"/>
                                                                                </LinearGradientBrush>
                                                                            </DatePicker.Background>
                                                                            <DatePicker.Resources>
                                                                                <Style TargetType="{x:Type DatePickerTextBox}">
                                                                                    <Setter Property="Text" Value="Select date" />
                                                                                    <Setter Property="Focusable" Value="False"/>
                                                                                    <!--<Setter Property="IsReadOnly" Value="True"/>-->
                                                                                    <!--<Setter Property="Background">
                                                                                        <Setter.Value>
                                                                                            <ImageBrush ImageSource="/LIT.Core;component/Media/Images/folder.png" Opacity="0"/>
                                                                                        </Setter.Value>
                                                                                    </Setter>-->
                                                                                </Style>
                                                                            </DatePicker.Resources>
                                                                        </DatePicker>
                                                                    </DataTemplate>
                                                                </DataGridTemplateColumn.CellTemplate>
                                                                <DataGridTemplateColumn.CellEditingTemplate>
                                                                    <DataTemplate>
                                                                        <Grid FocusManager.FocusedElement="{Binding ElementName= Txtvalidto}" >
                                                                            <DatePicker  x:Name="Txtvalidto" SelectedDate="{Binding ValidTo, UpdateSourceTrigger=PropertyChanged}" FontFamily="Calibri"  FontSize="14" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" BorderBrush="Transparent" BorderThickness="0,0,0,0">
                                                                                <DatePicker.Background>
                                                                                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                                                                        <GradientStop Color="White"/>
                                                                                        <GradientStop Color="White" Offset="1"/>
                                                                                    </LinearGradientBrush>
