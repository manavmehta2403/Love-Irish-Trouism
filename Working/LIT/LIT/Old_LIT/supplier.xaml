<UserControl x:Class="LIT.Old_LIT.Supplier"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:LIT"
      xmlns:mmlist="clr-namespace:SQLDataAccessLayer.Models;assembly=SQLDataAccessLayer"
      xmlns:behavior="clr-namespace:LIT.Core.Behaviors;assembly=LIT.Core"
      mc:Ignorable="d" 
      Height="Auto" Width="auto"  Background="Transparent" Margin="0,-1,0,0" >
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LIT.Core;component/Styles/PasswordBoxStyles.xaml"/>
                <ResourceDictionary Source="/LIT.Core;component/Styles/TextBoxStyles.xaml"/>
                <ResourceDictionary Source="/LIT.Core;component/Styles/ButtonStyles.xaml"/>
                <ResourceDictionary Source="/LIT.Core;component/Styles/TabItemStyles.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>

    <Grid> 
        <Grid VerticalAlignment="Top" Background="#579f00" x:Name="mysuppliergrid"  >
            <Grid.ColumnDefinitions>
                <!--<ColumnDefinition Width="297.83" />-->
                <ColumnDefinition />
                <ColumnDefinition MaxWidth="145" />
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="50"/>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>


            <DockPanel  Grid.Row="0" Grid.Column="0"  HorizontalAlignment="Left"  VerticalAlignment="Center" Margin="10,0,0,0">

                <TextBlock FontFamily="Calibri" FontSize="18" Foreground="White" FontWeight="SemiBold" x:Name="TxtblsuppNamewithpath" ></TextBlock>

            </DockPanel>
            <DockPanel Grid.Row="0" Grid.Column="1" Background="Transparent" HorizontalAlignment="Center" Width="150" Visibility="Collapsed" >

                <Button Style="{StaticResource btnBlue}" x:Name="BtnSupplierSave" Background="Transparent" Click="BtnSupplierSave_Click" FontFamily="Calibri" Height="30" FontSize="16" BorderBrush="Transparent" Foreground="#FF579F00" FontWeight="Bold" MaxWidth="79" Width="79" Cursor="Hand" MouseEnter="BtnSupplierSave_MouseEnter" MouseLeave="BtnSupplierSave_MouseLeave" Visibility="Collapsed" >

                    <StackPanel Orientation="Horizontal">
                        <Image Source="/LIT.Core;component/Media/Images/save.png" Width="16" VerticalAlignment="Center"></Image>
                        <Label Foreground="#FF579F00" FontSize="16" VerticalAlignment="Center">Save</Label>
                    </StackPanel>
                </Button>

                <Button  Style="{StaticResource btnBlue}" Click="BtnSupplierSaveandclose_Click" x:Name="BtnSupplierSaveandclose" FontFamily="Calibri" Height="30" Background="Transparent" FontSize="16" BorderBrush="#FF579F00" Foreground="#FF579F00" FontWeight="Bold"  MaxWidth="120" Width="120" BorderThickness="2,0,0,0" Cursor="Hand" MouseEnter="BtnSupplierSaveandclose_MouseEnter" MouseLeave="BtnSupplierSaveandclose_MouseLeave" HorizontalContentAlignment="Center" Margin="15,0,15,0" HorizontalAlignment="Center" Visibility="Collapsed" >
                    <StackPanel Orientation="Horizontal">
                        <Image Source="/LIT.Core;component/Media/Images/save.png" Width="16" VerticalAlignment="Center"></Image>
                        <Label Foreground="#FF579F00" FontSize="16" VerticalAlignment="Center">Save &amp; Close</Label>
                    </StackPanel>
                </Button>

            </DockPanel>

            <StackPanel Grid.Column="0" Grid.Row="1" Grid.ColumnSpan="3" Background="White" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"  >
                <TabControl Width="auto" Grid.Row="0" Height="auto" MinWidth="{Binding ElementName=Navigationframe, Path=ActualWidth}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"  Background="#4d4d4d" BorderBrush="#FF4D4D4D" BorderThickness="1,1,1,1"  x:Name="supWindow">

                    <TabItem Header="Supplier" FontFamily="Calibri" FontSize="16" Width="131"  Height="48"  Style="{StaticResource TabItemBase}"  Margin="-4,0,0,0" Cursor="Hand" >

                        <Border Background="#f5f5f5" HorizontalAlignment="Stretch" BorderBrush="#b9b9b9" BorderThickness="1,1,1,1"  CornerRadius="10" Margin="10,10,10,10" Padding="10, 10,10,10" Width="auto" Height="auto" x:Name="test" >

                            <Grid x:Name="formgridpass">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition MinWidth="162" Width="*"/>
                                    <ColumnDefinition MinWidth="162" Width="*"/>
                                    <ColumnDefinition MinWidth="162" Width="*"/>
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition MinHeight="48" Height="*"/>
                                    <RowDefinition MinHeight="48" Height="*"/>
                                    <RowDefinition MinHeight="30" Height="*"/>
                                    <RowDefinition MinHeight="48" Height="*"/>
                                    <RowDefinition MinHeight="48" Height="*"/>
                                    <RowDefinition MinHeight="40" Height="*"/>
                                    <RowDefinition MinHeight="48" Height="*"/>
                                    <RowDefinition MinHeight="48" Height="*"/>
                                    <RowDefinition MinHeight="48" Height="*"/>
                                    <RowDefinition MinHeight="48" Height="*"/>
                                    <RowDefinition MinHeight="140" Height="*"/>
                                </Grid.RowDefinitions>

                                <StackPanel Orientation="Vertical" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Height="50"  Grid.Column="0" Grid.Row="0" Margin="10,0,10,0">
                                    <Label Content="Supplier Name" FontFamily="Calibri" Width="auto" Height="22" FontSize="14"  Foreground="#FF53555E" Padding="0,0,0,0"  />
                                    <TextBox TabIndex="0" Template="{StaticResource TextBoxBaseControlTemplate}" x:Name="TxtSupplierName" FontFamily="Calibri" Width="auto" Height="24" FontSize="14" VerticalContentAlignment="Center" Padding="5,0,5,0" BorderBrush="#FFA1A1A1" PreviewTextInput="TxtSupplierName_PreviewTextInput" />
                                </StackPanel>

                                <StackPanel Orientation="Vertical" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Height="50" Grid.Column="1" Grid.Row="0" Margin="10,0,10,0">
                                    <Label Content="Supplier ID" FontFamily="Calibri" Width="auto" Height="22" FontSize="14" Foreground="#FF53555E" Padding="0,0,0,0"  />
                                    <TextBox TabIndex="1" Template="{StaticResource TextBoxBaseControlTemplate}" x:Name="TxtSupplierID" IsEnabled="False" FontFamily="Calibri" Width="auto" Height="24" FontSize="14" VerticalContentAlignment="Center" Padding="5,0,5,0" Background="#FFEAEAEA" BorderBrush="#FFA1A1A1"  />

                                </StackPanel>

                                <StackPanel Orientation="Horizontal"  HorizontalAlignment="Stretch" VerticalAlignment="Bottom" Height="40" Width="auto" Grid.Column="2" Grid.Row="0" Margin="25,0,5,0">
                                    <Label Content="Is Supplier Active" FontFamily="Calibri" Width="auto" Height="20" FontSize="14"  Foreground="#FF53555E" Padding="0,0,0,0"  />
                                    <CheckBox TabIndex="2" x:Name="ChbSupplieractive" IsEnabled="False" FontFamily="Calibri" Width="15" Height="24" FontSize="14" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" BorderBrush="#FFA1A1A1"  Margin="5,0,0,0" />
                                </StackPanel>

                                <StackPanel Orientation="Vertical" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Height="50"  Grid.Column="0" Grid.Row="1" Margin="10,0,10,0">
                                    <Label Content="Hosts" FontFamily="Calibri" Width="auto" Height="22" FontSize="14"  Foreground="#FF53555E" Padding="0,0,0,0"  />
                                    <TextBox TabIndex="3" Template="{StaticResource TextBoxBaseControlTemplate}" x:Name="TxtHosts" FontFamily="Calibri" Width="auto"  Height="24" FontSize="14" VerticalContentAlignment="Center" Padding="5,0,5,0" BorderBrush="#FFA1A1A1" PreviewTextInput="TxtHosts_PreviewTextInput" />
                                </StackPanel>

                                <StackPanel Orientation="Vertical" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Height="50"  Grid.Column="1" Grid.Row="1" Margin="10,0,10,0">
                                    <Label Content="Custom Code" FontFamily="Calibri" Width="auto" Height="22" FontSize="14"  Foreground="#FF53555E" Padding="0,0,0,0"  />
                                    <TextBox TabIndex="4" Template="{StaticResource TextBoxBaseControlTemplate}" x:Name="TxtCustomCode" FontFamily="Calibri" Width="auto"  Height="24" FontSize="14" VerticalContentAlignment="Center" Padding="5,0,5,0" BorderBrush="#FFA1A1A1" PreviewTextInput="TxtCustomCode_PreviewTextInput" />
                                </StackPanel>

                                <StackPanel Orientation="Vertical" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Height="20"  Grid.Column="0" Grid.Row="2" Margin="10,0,10,0">
                                    <Label  Content="Location" FontFamily="Calibri" Width="auto" Height="24" FontSize="20" Foreground="#FF25304C" Padding="0,0,0,0" FontWeight="Bold"  />
                                </StackPanel>

                                <StackPanel Orientation="Vertical" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Height="50"  Grid.Column="0" Grid.Row="3" Margin="10,0,10,0">
                                    <Label Content="Street" FontFamily="Calibri" Width="auto" Height="22" FontSize="14"  Foreground="#FF53555E" Padding="0,0,0,0"  />
                                    <TextBox TabIndex="5" Template="{StaticResource TextBoxBaseControlTemplate}" x:Name="TxtStreet" FontFamily="Calibri" Width="auto"  Height="24" FontSize="14" VerticalContentAlignment="Center" Padding="5,0,5,0" BorderBrush="#FFA1A1A1" />
                                </StackPanel>

                                <StackPanel Orientation="Vertical" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Height="50"  Grid.Column="1" Grid.Row="3" Margin="10,0,10,0">
                                    <Label Content="Country" FontFamily="Calibri" Width="auto" Height="22" FontSize="14"  Foreground="#FF53555E" Padding="0,0,0,0"  />
                                    <ComboBox TabIndex="6" x:Name="CmbCountry" FontFamily="Calibri" Width="auto"  Height="24" FontSize="14" VerticalContentAlignment="Center" Padding="5,0,5,0"  SelectionChanged="CmbCountry_SelectionChanged" BorderBrush="#FFA1A1A1">
                                    </ComboBox>
                                </StackPanel>

                                <StackPanel Orientation="Vertical" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Height="50"  Grid.Column="2" Grid.Row="3" Margin="10,0,10,0">
                                    <Label Content="State" FontFamily="Calibri" Width="auto" Height="22" FontSize="14"  Foreground="#FF53555E" Padding="0,0,0,0"  />
                                    <ComboBox TabIndex="7" x:Name="CmbState" FontFamily="Calibri" Width="auto"  Height="24" FontSize="14" VerticalContentAlignment="Center" Padding="5,0,5,0"   SelectionChanged="CmbState_SelectionChanged" BorderBrush="#FFA1A1A1">
                                    </ComboBox>
                                </StackPanel>

                                <StackPanel Orientation="Vertical" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Height="50"  Grid.Column="0" Grid.Row="4" Margin="10,0,10,0">
                                    <Label Content="Region" FontFamily="Calibri" Width="auto" Height="22" FontSize="14"  Foreground="#FF53555E" Padding="0,0,0,0"  />
                                    <ComboBox TabIndex="8" x:Name="CmbRegion" FontFamily="Calibri" Width="auto"  Height="24" FontSize="14" VerticalContentAlignment="Center" Padding="5,0,5,0" SelectionChanged="CmbRegion_SelectionChanged" BorderBrush="#FFA1A1A1">
                                    </ComboBox>
                                </StackPanel>

                                <StackPanel Orientation="Vertical" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Height="50"  Grid.Column="1" Grid.Row="4" Margin="10,0,10,0">
                                    <Label Content="City" FontFamily="Calibri" Width="auto" Height="22" FontSize="14"  Foreground="#FF53555E" Padding="0,0,0,0"  />
                                    <ComboBox TabIndex="9" x:Name="CmbCity" FontFamily="Calibri" Width="auto"  Height="24" FontSize="14" VerticalContentAlignment="Center" Padding="5,0,5,0" BorderBrush="#FFA1A1A1">
                                    </ComboBox>
                                </StackPanel>

                                <StackPanel Orientation="Vertical" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Height="50"  Grid.Column="2" Grid.Row="4" Margin="10,0,10,0">
                                    <Label Content="Post Code" FontFamily="Calibri" Width="auto" Height="22" FontSize="14"  Foreground="#FF53555E" Padding="0,0,0,0"  />
                                    <TextBox TabIndex="10" Template="{StaticResource TextBoxBaseControlTemplate}" x:Name="TxtPostCode" FontFamily="Calibri" Width="auto" Height="24" FontSize="14" Padding="5,0,5,0" VerticalAlignment="Center" VerticalContentAlignment="Center" >
                                    </TextBox>
                                </StackPanel>

                                <StackPanel Orientation="Vertical" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Height="25"  Grid.Column="0" Grid.Row="5" Margin="10,0,10,0">
                                    <Label Content="Contact" FontFamily="Calibri" Width="auto" Height="25" FontSize="20" Foreground="#FF25304C" Padding="0,0,0,0" FontWeight="Bold"  />
                                </StackPanel>


                                <StackPanel Orientation="Vertical" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Height="25"  Grid.Column="2" Grid.Row="5" Margin="10,0,0,0">
                                    <Label Content="Payment Terms" FontFamily="Calibri" Width="auto" Height="25" FontSize="20" Foreground="#FF25304C" Padding="0,0,0,0" FontWeight="Bold"  />
                                </StackPanel>

                                <StackPanel Orientation="Vertical" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Height="50"  Grid.Column="0" Grid.Row="6" Margin="10,0,10,0">
                                    <Label Content="Phone" FontFamily="Calibri" Width="auto" Height="22" FontSize="14"  Foreground="#FF53555E" Padding="0,0,0,0"  />
                                    <TextBox TabIndex="11" Template="{StaticResource TextBoxBaseControlTemplate}" x:Name="TxtPhone" FontFamily="Calibri" Width="auto"  Height="24" FontSize="14" VerticalContentAlignment="Center" Padding="5,0,5,0"  BorderBrush="#FFA1A1A1" >
                                    </TextBox>
                                </StackPanel>

                                <StackPanel Orientation="Vertical" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Height="50"  Grid.Column="1" Grid.Row="6" Margin="10,0,10,0">
                                    <Label Content="Free Ph" FontFamily="Calibri" Width="auto" Height="22" FontSize="14"  Foreground="#FF53555E" Padding="0,0,0,0"  />
                                    <TextBox TabIndex="12" Template="{StaticResource TextBoxBaseControlTemplate}" x:Name="TxtFreeph" FontFamily="Calibri" Width="auto"  Height="24" FontSize="14" VerticalContentAlignment="Center" Padding="5,0,5,0" BorderBrush="#FFA1A1A1" >
                                    </TextBox>
                                </StackPanel>





                                <StackPanel Orientation="Vertical" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Height="50"  Grid.Column="2" Grid.Row="6" Margin="10,0,10,0">
                                    <Label Content="Payment Terms (in Days)" FontFamily="Calibri" Width="auto" Height="22" FontSize="14"  Foreground="#FF53555E" Padding="0,0,0,0"  />
                                    <TextBox TabIndex="13" GotFocus="PaymentTerms_GotFocus"  LostFocus="PaymentTerms_LostFocus" Template="{StaticResource TextBoxBaseControlTemplate}" x:Name="PaymentTerms" FontFamily="Calibri" Width="auto" Height="24" FontSize="14" VerticalContentAlignment="Center" HorizontalContentAlignment="Right" Padding="5,0,5,0" BorderBrush="#FFA1A1A1">

                                    </TextBox>
                                </StackPanel>

                                <StackPanel Orientation="Vertical" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Height="50"  Grid.Column="0" Grid.Row="7" Margin="10,0,10,0">
                                    <Label Content="Mobile" FontFamily="Calibri" Width="auto" Height="22" FontSize="14"  Foreground="#FF53555E" Padding="0,0,0,0"  />
                                    <TextBox TabIndex="14" Template="{StaticResource TextBoxBaseControlTemplate}" x:Name="TxtMobile" FontFamily="Calibri" Width="auto" Height="24" FontSize="14" VerticalContentAlignment="Center" Padding="5,0,5,0" BorderBrush="#FFA1A1A1">
                                    </TextBox>
                                </StackPanel>

                                <StackPanel Orientation="Vertical" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Height="50"  Grid.Column="1" Grid.Row="7" Margin="10,0,10,0">
                                    <Label Content="Fax" FontFamily="Calibri" Width="auto" Height="22" FontSize="14"  Foreground="#FF53555E" Padding="0,0,0,0"  />
                                    <TextBox TabIndex="15" Template="{StaticResource TextBoxBaseControlTemplate}" x:Name="TxtFax" FontFamily="Calibri" Width="auto" Height="24" FontSize="14" VerticalContentAlignment="Center" Padding="5,0,5,0" BorderBrush="#FFA1A1A1" >
                                    </TextBox>
                                </StackPanel>
                                <StackPanel Orientation="Vertical" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Height="50"  Grid.Column="2" Grid.Row="7" Margin="10,0,10,0">
                                    <Label Content="Deposit Amount (in €)" FontFamily="Calibri" Width="auto" Height="22" FontSize="14"  Foreground="#FF53555E" Padding="0,0,0,0"  />
                                    <TextBox TabIndex="16" LostFocus="DepositAmount_LostFocus" GotFocus="DepositAmount_GotFocus" Template="{StaticResource TextBoxBaseControlTemplate}" x:Name="DepositAmount" FontFamily="Calibri" Width="auto" Height="24" FontSize="14" VerticalContentAlignment="Center" HorizontalContentAlignment="Right" Padding="5,0,5,0" BorderBrush="#FFA1A1A1">
                                    </TextBox>
                                </StackPanel>

                                <StackPanel Orientation="Vertical" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Height="100" Grid.RowSpan="2" Grid.Column="0" Grid.Row="8" Margin="10,0,10,0">
                                    <Label Content="Postal Address" FontFamily="Calibri" Width="auto" Height="22" FontSize="14"  Foreground="#FF53555E" Padding="0,0,0,0"  />
                                    <TextBox TabIndex="17" Template="{StaticResource TextBoxBaseControlTemplate}" x:Name="TxtPostalAddress" FontFamily="Calibri" Width="auto" Height="75" FontSize="14" Padding="5,0,5,0" VerticalAlignment="Center" BorderBrush="#FFA1A1A1" TextWrapping="Wrap" AcceptsReturn="True"  VerticalScrollBarVisibility="Visible">
                                    </TextBox>
                                </StackPanel>


                              



                               

                                <StackPanel Orientation="Vertical" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Height="50"  Grid.Column="1" Grid.Row="8" Margin="10,0,10,0">
                                    <Label Content="Email *" FontFamily="Calibri" Width="auto" Height="22" FontSize="14"  Foreground="#FF53555E" Padding="0,0,0,0"  />
                                    <TextBox TabIndex="18" Template="{StaticResource TextBoxBaseControlTemplate}" x:Name="TxtEmail" FontFamily="Calibri" Width="auto" Height="24" FontSize="14" VerticalContentAlignment="Center" Padding="5,0,5,0" BorderBrush="#FFA1A1A1">
                                    </TextBox>
                                </StackPanel>


                                <StackPanel Orientation="Vertical" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Height="50"  Grid.Column="1" Grid.Row="9" Margin="10,0,10,0">
                                    <Label Content="Website" FontFamily="Calibri" Width="auto" Height="22" FontSize="14"  Foreground="#FF53555E" Padding="0,0,0,0"  />
                                    <TextBox TabIndex="19" Template="{StaticResource TextBoxBaseControlTemplate}" x:Name="TxtWebsite" FontFamily="Calibri" Width="auto" Height="24" FontSize="14" VerticalContentAlignment="Center" Padding="5,0,5,0" BorderBrush="#FFA1A1A1" >
                                    </TextBox>
                                </StackPanel>


                                <StackPanel Orientation="Vertical" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Height="auto" Grid.ColumnSpan="3" Grid.Column="0" Grid.Row="10" Margin="5,10,5,0">
                                    <Grid>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition MinWidth="162" Width="*"/>
                                            <ColumnDefinition MinWidth="162" Width="*"/>
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition MinHeight="150" />
                                        </Grid.RowDefinitions>

                                        <StackPanel  Grid.Column="0" Grid.Row="0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="10,0,10,0">
                                            <Label Content="Comments" FontFamily="Calibri" Width="auto" Height="22"  FontWeight="Bold" FontSize="20" Foreground="#FF25304C" Padding="0,0,0,0" Margin="0,0,0,5" />
                                            <TextBox TabIndex="20" Template="{StaticResource TextBoxBaseControlTemplate}" x:Name="TxtComments" FontFamily="Calibri" Width="auto" Height="130" FontSize="14" Padding="5,0,5,0" BorderBrush="#FFA1A1A1" TextWrapping="Wrap" AcceptsReturn="True"  VerticalScrollBarVisibility="Visible" VerticalAlignment="Center">
                                            </TextBox>

                                        </StackPanel>
                                        <StackPanel Orientation="Vertical"  HorizontalAlignment="Stretch" VerticalAlignment="Top"   Grid.Column="1" Grid.Row="0" Margin="10,0,10,0">
                                            <Label Content="Description" FontFamily="Calibri" Width="auto" Height="22"  FontWeight="Bold" FontSize="20" Foreground="#FF25304C" Padding="0,0,0,0" Margin="0,0,0,5" />
                                            <TextBox TabIndex="21" Template="{StaticResource TextBoxBaseControlTemplate}" x:Name="TxtDescription" FontFamily="Calibri" Width="auto" Height="130" FontSize="14"  Padding="5,0,5,0" BorderBrush="#FFA1A1A1" TextWrapping="Wrap" AcceptsReturn="True"  VerticalScrollBarVisibility="Visible" VerticalAlignment="Center">
                                            </TextBox>
                                        </StackPanel>
                                    </Grid>
                                </StackPanel>



                                <TextBox x:Name="hdnSupplierid" Visibility="Hidden"></TextBox>
                                <TextBox x:Name="hdnSuppliermode" Visibility="Hidden"></TextBox>
                                <TextBox x:Name="TxtSupplierFolderPath" Visibility="Hidden"></TextBox>
                                <TextBox x:Name="hdnSupplierAutoId" Visibility="Hidden"></TextBox>
                            </Grid>

                        </Border>

                    </TabItem>

                    <TabItem Header="Services" Style="{StaticResource TabItemBase}" Width="131" FontSize="16" Height="48" Margin="0,0,0,0" Cursor="Hand" >
                        <StackPanel>
                            <StackPanel>
                                <Label x:Name="lblserviceName" Visibility="Hidden" HorizontalAlignment="Right" FontWeight="Medium" FontSize="13" Margin="0 0 10 2" Foreground="#FFF7F0F0"></Label>
                                <Label x:Name="lblRatesDate" Visibility="Hidden" HorizontalAlignment="Right" FontWeight="Medium" FontSize="13" Margin="0 -10 10 0" Foreground="#FFF7F0F0"></Label>
                            </StackPanel>
                            <Border Background="#f5f5f5" x:Name="insideServicesTb" BorderBrush="Transparent" BorderThickness="1,1,1,1" MinHeight="{Binding ElementName=Navigationframe, Path=ActualHeight}" CornerRadius="10" Margin="5" Padding="5" HorizontalAlignment="Stretch" VerticalAlignment="Top" >
                                <TabControl HorizontalAlignment="Stretch" VerticalAlignment="Top"  Background="White" >
                                    <TabItem Header="Services" Name="SupplierServices" Style="{StaticResource TabItemWhite}" Width="131" FontSize="16" Height="48" Cursor="Hand" Margin="-4,0,0,0" >

                                        <StackPanel x:Name="StackServices">
                                            <StackPanel Visibility="Visible" HorizontalAlignment="Right" Orientation="Horizontal" Panel.ZIndex="2">

                                                <CheckBox x:Name="chbhidenonactive"  IsThreeState="False" Content="Hide Non Active Items" IsChecked="True" Click="chbhidenonactive_Click" FontSize="12" FontWeight="Bold" Foreground="#ff53555E" Margin="0,9,75,10" ></CheckBox>


                                            </StackPanel>
                                            <StackPanel Margin="0,-38,0,0">
                                                <StackPanel Orientation="Vertical" Width="auto" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                                                    <Label  FontSize="14" Content="Services" VerticalAlignment="Center" FontWeight="Medium" Foreground="#25304C" Height="25" Margin="10,5,10,10" Width="auto" />
                                                    <Button  Style="{StaticResource btnBlue}"  Name="BtnServiceAdd" Click="BtnServiceAdd_Click" Margin="-20,-44,0,0" Background="Transparent" VerticalAlignment="Center"  VerticalContentAlignment="Center" FontFamily="Calibri" Height="25" FontSize="16" BorderBrush="#FF579F00" Foreground="#FF579F00" FontWeight="Bold" MaxWidth="140" Width="58" Cursor="Hand" HorizontalAlignment="Right">

                                                        <StackPanel Orientation="Horizontal" VerticalAlignment="Center">
                                                            <Image Source="/LIT.Core;component/Media/Images/plus.png" Width="13" VerticalAlignment="Center"></Image>
                                                            <Label Foreground="#FF579F00" FontSize="16" VerticalAlignment="Center" Margin="0,-1,0,0">Add</Label>
                                                        </StackPanel>

                                                    </Button>
                                                </StackPanel>
                                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" MinHeight="372" MaxHeight="370"  Grid.Row="0" Background="white">

                                                    <DataGrid x:Name="dgSupplierServices" BorderThickness="1,1,1,1" BorderBrush="#c9c9c9" AutoGenerateColumns="False" CanUserAddRows="False" HorizontalGridLinesBrush="#C9C9C9" VerticalGridLinesBrush="#C9C9C9" Width="{Binding ElementName=StackServices, Path=ActualWidth}" Background="White"
                                                               SelectedCellsChanged="dgSupplierServices_SelectedCellsChanged"
                          SelectionMode="Extended" RowDetailsVisibilityMode="VisibleWhenSelected"
                                                             behavior:OldSingleClickDataGridBehavior.EnableSingleClickEditing="True"  >
                                                        <DataGrid.Resources>
                                                            <Style TargetType="{x:Type DataGridCell}">
                                                                <EventSetter Event="MouseDoubleClick" Handler="dgSupplierServicesDataGridCell_MouseDoubleClick"/>
                                                            </Style>
                                                        </DataGrid.Resources>
                                                        <DataGrid.Columns>

                                                            <DataGridTextColumn Header="ServiceID" Binding="{Binding ServiceID}"  Visibility="Hidden" Width="0">
                                                                <DataGridTextColumn.HeaderStyle>
                                                                    <Style TargetType="DataGridColumnHeader">
                                                                        <Setter Property="HorizontalContentAlignment" Value="Center" />
                                                                    </Style>
                                                                </DataGridTextColumn.HeaderStyle>

                                                            </DataGridTextColumn>
                                                            <DataGridTextColumn Header="SupplierID" Binding="{Binding SupplierID}" Visibility="Hidden" Width="0">
                                                                <DataGridTextColumn.CellStyle>
                                                                    <Style>
                                                                        <!--<Setter Property="ToolTip" Value="{Binding SupplierID}">
                                                    
                                                </Setter>-->
                                                                    </Style>
                                                                </DataGridTextColumn.CellStyle>
