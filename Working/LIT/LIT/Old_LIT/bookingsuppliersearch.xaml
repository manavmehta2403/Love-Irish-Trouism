<Window x:Class="LIT.Old_LIT.BookingSupplierSearch"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:old_TimePicker="http://schemas.xceed.com/wpf/xaml/toolkit"
        xmlns:controls="clr-namespace:LIT.Core.Controls;assembly=LIT.Core"
        xmlns:behavior="clr-namespace:LIT.Core.Behaviors;assembly=LIT.Core"
        xmlns:local="clr-namespace:LIT"          
        mc:Ignorable="d"
        Title="Add New Booking" MinHeight="700" MinWidth="700" WindowState="Maximized" Background="white" WindowStartupLocation="CenterScreen" >

    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LIT.Core;component/Styles/PasswordBoxStyles.xaml"/>
                <ResourceDictionary Source="/LIT.Core;component/Styles/TextBoxStyles.xaml"/>
                <ResourceDictionary Source="/LIT.Core;component/Styles/ButtonStyles.xaml"/>
                <ResourceDictionary Source="/LIT.Core;component/Styles/ScrollBarStyles.xaml"/>
                <ResourceDictionary Source="/LIT.Core;component/Styles/TabItemStyles.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Window.Resources>

    <Grid Background="#f5f5f5">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="5*"/>
            <ColumnDefinition Width="67*"/>
        </Grid.ColumnDefinitions>
        <Label Grid.ColumnSpan="2" >
        </Label>

        <StackPanel Grid.ColumnSpan="2">
            <Label x:Name="lblserviceName" Visibility="Hidden" HorizontalAlignment="Right" FontWeight="Medium" FontSize="13" Margin="0 0 10 2"></Label>
            <Label x:Name="lblRatesDate" Visibility="Hidden" HorizontalAlignment="Right" FontWeight="Medium" FontSize="13" Margin="0 -10 10 0"></Label>
        </StackPanel>

        <TabControl Width="auto" x:Name="tbcntBkgsrch" MinHeight="570" Height="auto" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="10" Background="#f9f9f9" BorderBrush="#dadada" Grid.ColumnSpan="2">

            <TabItem Header="Supplier Services Search" Name="TbSuppServiceSrch" FontFamily="Calibri" FontSize="16" Width="auto"  Height="45"  VerticalAlignment="Bottom" Style="{StaticResource TabItemLightGrey}"   Cursor="Hand" Margin="-2,0,0,0" >
                <Grid Background="#f5f5f5">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="300" />
                        <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*" />
                    </Grid.RowDefinitions>


                    <StackPanel  Grid.Row="0" Grid.Column="0"  VerticalAlignment="Stretch" Height="auto" Background="#f9f9f9">


                        <StackPanel Orientation="Vertical" HorizontalAlignment="Stretch" VerticalAlignment="Top" Height="50" Width="auto" Margin="10,10,10,0" >
                            <Label Content="Supplier Name" FontFamily="Calibri" Width="auto" Height="22" FontSize="14"  Foreground="#FF53555E" Padding="0,0,0,0"  />
                            <TextBox Template="{StaticResource TextBoxBaseControlTemplate}" x:Name="TxtSupplierName" FontFamily="Calibri" MinWidth="223" Width="auto" Height="24" FontSize="14" VerticalContentAlignment="Center" Padding="5,0,5,0" VerticalAlignment="Center" BorderBrush="#FFA1A1A1" KeyUp="TxtSupplierName_KeyUp" />
                        </StackPanel>

                        <StackPanel Orientation="Vertical" HorizontalAlignment="Stretch" VerticalAlignment="Center" Height="50" Width="auto" Margin="10,10,10,0">
                            <Label Content="Service" FontFamily="Calibri" Width="auto" Height="22" FontSize="14" Foreground="#FF53555E" Padding="0,0,0,0"  />
                            <ComboBox  x:Name="CmbServiceType" FontFamily="Calibri" MinWidth="223" Width="auto" Height="25" FontSize="14" Padding="5,0,5,0" Background="#FFEAEAEA" BorderBrush="#FFA1A1A1" VerticalAlignment="Center" VerticalContentAlignment="Center" />
                        </StackPanel>

                        <StackPanel Orientation="Vertical" HorizontalAlignment="Stretch" VerticalAlignment="Center" Height="50" Width="auto" Margin="10,10,10,0">
                            <Label Content="Region" FontFamily="Calibri" Width="auto" Height="22" FontSize="14"  Foreground="#FF53555E" Padding="0,0,0,0"  />
                            <ComboBox x:Name="CmbRegion" FontFamily="Calibri" MinWidth="223" Width="auto" Height="25" FontSize="14" VerticalContentAlignment="Center" Padding="5,0,5,0" VerticalAlignment="Center" BorderBrush="#FFA1A1A1">
                            </ComboBox>
                        </StackPanel>

                        <StackPanel Orientation="Vertical" HorizontalAlignment="Stretch" VerticalAlignment="Center" Height="50" Width="auto" Margin="10,10,10,0">
                            <Label Content="City" FontFamily="Calibri" Width="auto" Height="22" FontSize="14"  Foreground="#FF53555E" Padding="0,0,0,0"  />
                            <ComboBox x:Name="CmbCity" FontFamily="Calibri" MinWidth="223" Width="auto" Height="25" FontSize="14" VerticalContentAlignment="Center" Padding="5,0,5,0" VerticalAlignment="Center"  BorderBrush="#FFA1A1A1">
                            </ComboBox>
                        </StackPanel>
                        <StackPanel HorizontalAlignment="Stretch" VerticalAlignment="Stretch" >
                            <Grid  HorizontalAlignment="Left" Margin="20,60,0,0" VerticalAlignment="Bottom" Height="auto" Width="300">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="90" />
                                    <ColumnDefinition  Width="90" />
                                    <ColumnDefinition  Width="90" />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*"/>
                                </Grid.RowDefinitions>

                                <Button Content="Search" Grid.Column="0" Grid.Row="0" Template="{StaticResource ButtonBaseTemplate}"  Padding="22,4,5,5" x:Name="btnSearch" HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="0,0,0,0" VerticalContentAlignment="Center" FontFamily="Calibri" Height="28" FontSize="14"  BorderBrush="#c9c9c9" Foreground="#252e39" FontWeight="Normal" MaxWidth="140" Width="80" Cursor="Hand" Click="btnSearch_Click">
                                    <Button.Background>
                                        <SolidColorBrush Color="white" Opacity="1"/>
                                    </Button.Background>
                                    <Button.Resources>
                                        <Style TargetType="{x:Type Border}">
                                            <Setter Property="CornerRadius" Value="3"/>
                                        </Style>
                                    </Button.Resources>
                                </Button>
                                <Button Content="View All" Grid.Column="1" Grid.Row="0" Template="{StaticResource ButtonBaseTemplate}"  Padding="18,4,5,5" x:Name="btnViewallSearch" HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="5,5,0,0" VerticalContentAlignment="Center" FontFamily="Calibri" Height="28" FontSize="14"  BorderBrush="#c9c9c9" Foreground="#252e39" FontWeight="Normal" MaxWidth="140" Width="80" Cursor="Hand" Click="btnViewallSearch_Click">
                                    <Button.Background>
                                        <SolidColorBrush Color="white" Opacity="1"/>
                                    </Button.Background>
                                    <Button.Resources>
                                        <Style TargetType="{x:Type Border}">
                                            <Setter Property="CornerRadius" Value="3"/>
                                        </Style>
                                    </Button.Resources>
                                </Button>

                                <Button Content="Clear"  Grid.Row="0" Grid.Column="2" Template="{StaticResource ButtonBaseTemplate}"  Padding="22,4,5,5" HorizontalAlignment="Left" x:Name="btnClear" VerticalAlignment="Bottom" Margin="10,5,0,0" VerticalContentAlignment="Center" FontFamily="Calibri" Height="28" FontSize="14"  BorderBrush="#c9c9c9" Foreground="#252e39" FontWeight="Normal" MaxWidth="100" Width="80" Cursor="Hand" Click="btnClear_Click">
                                    <Button.Background>
                                        <SolidColorBrush Color="white" Opacity="1"/>
                                    </Button.Background>
                                    <Button.Resources>
                                        <Style TargetType="{x:Type Border}">
                                            <Setter Property="CornerRadius" Value="3"/>
                                        </Style>
                                    </Button.Resources>
                                </Button>
                            </Grid>
                        </StackPanel>

                    </StackPanel>
                    <Grid Grid.Row="0" Grid.Column="1" HorizontalAlignment="Stretch" VerticalAlignment="Top" Margin="0,0,0,0" Grid.RowSpan="2"  Background="#f6f6f6" x:Name="searchTable">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*"></RowDefinition>
                            <RowDefinition Height="80"></RowDefinition>
                        </Grid.RowDefinitions>
                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Height="auto"  Grid.Row="0" Background="White" >

                            <DataGrid behavior:NewSingleClickDataGridBehavior.EnableSingleClickEditing="True" x:Name="dgBkgSupplierService" Background="#fff" BorderBrush="#d9d9d9" BorderThickness="1,1,1,1" AutoGenerateColumns="False" HorizontalGridLinesBrush="#d9d9d9" VerticalGridLinesBrush="#d9d9d9" Margin="0,0,0,0" HorizontalAlignment="Stretch" Width="{Binding ElementName=searchTable , Path=ActualWidth}"  >
                                <DataGrid.Resources>
                                    <Style TargetType="{x:Type DataGridRow}">
                                        <EventSetter Event="MouseDoubleClick" Handler="dgBkgSupplierServiceDataGridCell_MouseDoubleClick"/>
                                    </Style>
                                </DataGrid.Resources>
                                <DataGrid.Columns>
                                    <DataGridTextColumn Header="ServiceID" Binding="{Binding ServiceID}"  Visibility="Hidden" Width="0"/>
                                    <DataGridTextColumn Header="SupplierID" Binding="{Binding SupplierID}" Visibility="Hidden" Width="0"/>
                                    <DataGridTextColumn Header="ServiceTypeID" Binding="{Binding ServiceTypeID}" Visibility="Hidden" Width="0"/>
                                    <DataGridTextColumn Header="Supplier Name" Binding="{Binding SupplierName}"    MinWidth="178" Width="*">
                                        <DataGridTextColumn.HeaderStyle>
                                            <Style TargetType="{x:Type DataGridColumnHeader}">
                                                <Setter Property="Foreground" Value="#252e39" />
                                                <Setter Property="HorizontalAlignment" Value="Stretch" />
                                                <Setter Property="VerticalAlignment" Value="Stretch" />
                                                <Setter Property="HorizontalContentAlignment" Value="Center" />
                                                <Setter Property="VerticalContentAlignment" Value="Center" />
                                                <Setter Property="IsTabStop" Value="False" />
                                                <Setter Property="SeparatorBrush" Value="#dadada" />
                                                <Setter Property="Padding" Value="4" />
                                            </Style>
                                        </DataGridTextColumn.HeaderStyle>
                                    </DataGridTextColumn>
                                    <DataGridTextColumn Header="Service Name" Binding="{Binding ServiceName}"   MinWidth="179" Width="*">
                                        <DataGridTextColumn.HeaderStyle>
                                            <Style TargetType="{x:Type DataGridColumnHeader}">
                                                <Setter Property="Foreground" Value="#252e39" />
                                                <Setter Property="HorizontalAlignment" Value="Stretch" />
                                                <Setter Property="VerticalAlignment" Value="Stretch" />
                                                <Setter Property="HorizontalContentAlignment" Value="Center" />
                                                <Setter Property="VerticalContentAlignment" Value="Center" />
                                                <Setter Property="IsTabStop" Value="False" />
                                                <Setter Property="SeparatorBrush" Value="#dadada" />
                                                <Setter Property="Padding" Value="4" />
                                            </Style>
                                        </DataGridTextColumn.HeaderStyle>
                                    </DataGridTextColumn>

                                </DataGrid.Columns>
                                <DataGrid.Style>
                                    <Style TargetType="DataGrid">
                                        <Style.Triggers>
                                            <DataTrigger Binding="{Binding HasItems, RelativeSource={RelativeSource Self}}" Value="false">
                                                <Setter Property="Template">
                                                    <Setter.Value>
                                                        <ControlTemplate TargetType="DataGrid">
                                                            <Grid>
                                                                <Grid.RowDefinitions>
                                                                    <RowDefinition Height="Auto" />
                                                                    <RowDefinition Height="*" />
                                                                </Grid.RowDefinitions>
                                                                <DataGridColumnHeadersPresenter x:Name="PART_ColumnHeadersPresenter" Grid.Column="1" Visibility="{Binding HeadersVisibility, ConverterParameter={x:Static DataGridHeadersVisibility.Column}, Converter={x:Static DataGrid.HeadersVisibilityConverter}, RelativeSource={RelativeSource AncestorType={x:Type DataGrid}}}"/>
                                                                <TextBlock Text="No records found..." Grid.Row="1" />
                                                            </Grid>
                                                        </ControlTemplate>
                                                    </Setter.Value>
                                                </Setter>
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </DataGrid.Style>
                            </DataGrid>
                        </StackPanel>
                    </Grid>

                    <Border BorderThickness="0,0,0,0" Background="#dadada" Height="1" VerticalAlignment="Bottom" Grid.ColumnSpan="2" Margin="0,0,0,-25"></Border>
                    <Grid  HorizontalAlignment="Right" Grid.Column="0" Grid.ColumnSpan="2"  VerticalAlignment="Bottom" Height="40" Width="655" Grid.Row="2" Margin="0,0,0,20">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition  />
                            <ColumnDefinition />
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition/>
                        </Grid.RowDefinitions>
                        <StackPanel VerticalAlignment="Center" Margin="0,0,0,0" Width="670" Grid.ColumnSpan="2">
                            <Grid VerticalAlignment="Center">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*" />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*"/>
                                </Grid.RowDefinitions>
                                <Button Content="Select" Name="btnSelect" Grid.Column="1" Grid.Row="0" Template="{StaticResource ButtonBaseTemplate}"  Padding="22,4,5,5" HorizontalAlignment="Right" VerticalAlignment="Center" Margin="0,0,120,0" VerticalContentAlignment="Center" FontFamily="Calibri" Height="28" FontSize="14"  BorderBrush="#c9c9c9" Foreground="#252e39" FontWeight="Normal" MaxWidth="140" Width="80" Cursor="Hand" Click="btnSelect_Click">
                                    <Button.Background>
                                        <SolidColorBrush Color="White" Opacity="1"/>
                                    </Button.Background>
                                    <Button.Resources>
                                        <Style TargetType="{x:Type Border}">
                                            <Setter Property="CornerRadius" Value="3"/>
                                        </Style>
                                    </Button.Resources>
                                </Button>

                                <Button Content="Cancel" Template="{StaticResource ButtonBaseTemplate}"  Padding="20,4,5,5" Name="btnCancel" Grid.Row="0" Grid.Column="1" HorizontalAlignment="Right" Margin="0,0,20,0" VerticalContentAlignment="Center" FontFamily="Calibri" Height="28" FontSize="14"  BorderBrush="#c9c9c9" Foreground="#252e39" FontWeight="Normal" MaxWidth="140" Width="80" Cursor="Hand" Click="btnCancel_Click" HorizontalContentAlignment="Center">
                                    <Button.Background>
                                        <SolidColorBrush Color="White" Opacity="1"/>
                                    </Button.Background>
                                    <Button.Resources>
                                        <Style TargetType="{x:Type Border}">
                                            <Setter Property="CornerRadius" Value="3"/>
                                        </Style>
                                    </Button.Resources>
                                </Button>
                            </Grid>
                        </StackPanel>
                    </Grid>

                </Grid>


            </TabItem>
            <TabItem Header="Select for Booking" Name="TbSelectforbooking" FontFamily="Calibri" FontSize="16" Width="180"  Height="45"  VerticalAlignment="Bottom" Style="{StaticResource TabItemLightGrey}"   Cursor="Hand" >
                <TabControl >
                    <TabItem Header="Services" Name="SupplierServices" Style="{StaticResource TabItemWhite}" Width="131" FontSize="16" Height="48" Cursor="Hand" Margin="-4,0,0,0" >



                        <DockPanel>
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="auto"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*"></RowDefinition>
                                </Grid.RowDefinitions>

                                <StackPanel Grid.Column="0" x:Name="StackServices" DockPanel.Dock="Left" Background="#f9f9f9" VerticalAlignment="Stretch">

                                    <StackPanel VerticalAlignment="Center">
                                        <Label  FontSize="14" Content="Services" 
            FontWeight="Medium" Foreground="#25304C" Height="30" Width="auto" />

                                        <Button  Style="{StaticResource btnBlue}" x:Name="BtnServicebkssAdd" Click="BtnServicebkssAdd_Click" 
             Margin="-20,-25,0,0" Background="Transparent" VerticalAlignment="Center"  
             VerticalContentAlignment="Center" FontFamily="Calibri" Height="30" FontSize="16"
             BorderBrush="#FF579F00" Foreground="#FF579F00" FontWeight="Bold" MaxWidth="140"
             Width="58" Cursor="Hand" HorizontalAlignment="Right">

                                            <StackPanel Orientation="Horizontal">

                                                <Label Foreground="#FF579F00" FontSize="16" VerticalAlignment="Center">&#43; Add</Label>
                                            </StackPanel>

                                        </Button>
                                    </StackPanel>
                                    <StackPanel>
                                        <DataGrid x:Name="dgSupplierServicesBkss" HorizontalScrollBarVisibility="Visible"  MinHeight="315" MaxHeight="650" VerticalAlignment="Stretch" VerticalScrollBarVisibility="Auto" BorderThickness="1,1,1,0" BorderBrush="#f9f9f9" AutoGenerateColumns="False" CanUserAddRows="False" HorizontalGridLinesBrush="#C9C9C9" VerticalGridLinesBrush="#C9C9C9" Width="{Binding ElementName=StackServices, Path=ActualWidth}" Background="#f9f9f9"
                                                 behavior:OldSingleClickDataGridBehavior.EnableSingleClickEditing="True" IsSynchronizedWithCurrentItem = "true"
                                                 >
                                            <DataGrid.Resources>
                                                <Style TargetType="{x:Type DataGridCell}">
                                                    <EventSetter Event="MouseDoubleClick" Handler="dgSupplierServicesBkssDataGridCell_MouseDoubleClick"/>
                                                </Style>
                                            </DataGrid.Resources>
                                            <DataGrid.Columns>

                                                <DataGridTextColumn Header="ServiceID" Binding="{Binding ServiceID}"  Visibility="Hidden" Width="0">
                                                    <DataGridTextColumn.HeaderStyle>
                                                        <Style TargetType="DataGridColumnHeader">
                                                            <Setter Property="HorizontalContentAlignment" Value="Center" />
                                                        </Style>
                                                    </DataGridTextColumn.HeaderStyle>

                                                </DataGridTextColumn>
                                                <DataGridTextColumn Header="SupplierID" Binding="{Binding SupplierID}" Visibility="Hidden" Width="0">
                                                    <DataGridTextColumn.CellStyle>
                                                        <Style>
                                                            <!--<Setter Property="ToolTip" Value="{Binding SupplierID}">
                                                    
                                                </Setter>-->
                                                        </Style>
                                                    </DataGridTextColumn.CellStyle>
                                                </DataGridTextColumn>
                                                <DataGridCheckBoxColumn Header="Active" x:Name="chbsupplieractive" Binding="{Binding IsActive, UpdateSourceTrigger=PropertyChanged}" Visibility="Hidden" >
                                                    <DataGridCheckBoxColumn.HeaderStyle>
                                                        <Style TargetType="DataGridColumnHeader">
                                                            <Setter Property="HorizontalContentAlignment" Value="Center" />
                                                            <Setter Property="Foreground" Value="#252E39" />
                                                            <Setter Property="FontSize" Value="14" />
                                                            <Setter Property="FontWeight" Value="Medium" />
                                                            <Setter Property="FontFamily" Value="Calibri" />
                                                        </Style>
                                                    </DataGridCheckBoxColumn.HeaderStyle>
                                                    <DataGridCheckBoxColumn.ElementStyle >
                                                        <Style TargetType="CheckBox">
                                                            <Setter Property="IsChecked" Value="{Binding IsActive}"></Setter>
                                                            <Setter Property="HorizontalAlignment" Value="Center" />
                                                            <Setter Property="HorizontalContentAlignment" Value="Center" />
                                                            <Setter Property="VerticalAlignment" Value="Center" />
                                                            <Setter Property="FontFamily" Value="Calibri" />
                                                            <Setter Property="FontSize" Value="14" />
                                                            <Setter Property="IsEnabled" Value="False"></Setter>
                                                            <!--<Setter Value="true" />-->
                                                        </Style>
                                                    </DataGridCheckBoxColumn.ElementStyle>
                                                    <DataGridCheckBoxColumn.EditingElementStyle>
                                                        <Style TargetType="CheckBox">
                                                            <Setter Property="IsChecked" Value="{Binding IsActive, Mode=OneWay}"/>
                                                            <Setter Property="HorizontalAlignment" Value="Center" />
                                                            <Setter Property="HorizontalContentAlignment" Value="Center" />
                                                            <Setter Property="VerticalAlignment" Value="Center" />
                                                            <Setter Property="FontFamily" Value="Calibri" />
                                                            <Setter Property="IsEnabled" Value="False"></Setter>
                                                        </Style>
                                                    </DataGridCheckBoxColumn.EditingElementStyle>

                                                </DataGridCheckBoxColumn>
                                                <DataGridTextColumn Header="Service Name" Binding="{Binding ServiceName,UpdateSourceTrigger=LostFocus}" Width="*" MinWidth="255" >
                                                    <DataGridTextColumn.HeaderStyle>
                                                        <Style TargetType="DataGridColumnHeader">
                                                            <Setter Property="HorizontalContentAlignment" Value="Center" />
                                                            <Setter Property="Foreground" Value="#252E39" />
                                                            <Setter Property="FontSize" Value="14" />
                                                            <Setter Property="FontWeight" Value="Medium" />
                                                            <Setter Property="FontFamily" Value="Calibri" />

                                                        </Style>
                                                    </DataGridTextColumn.HeaderStyle>
                                                    <DataGridTextColumn.ElementStyle>
                                                        <Style TargetType="TextBlock">
                                                            <Setter Property="HorizontalAlignment" Value="Left" />
                                                            <Setter Property="VerticalAlignment" Value="Center" />
                                                            <Setter Property="FontFamily" Value="Calibri" />
                                                            <Setter Property="FontSize" Value="14" />
                                                            <Setter Property="Padding" Value="5 0 0 0" />
                                                        </Style>
                                                    </DataGridTextColumn.ElementStyle>
                                                </DataGridTextColumn>

                                                <DataGridComboBoxColumn Header="Type" x:Name="CmbSupplierServiceType"  DisplayMemberPath="."
                                             SelectedItemBinding="{Binding SelectedItem, UpdateSourceTrigger=PropertyChanged}" Width="*" MinWidth="180"  >
                                                    <DataGridComboBoxColumn.HeaderStyle>
                                                        <Style TargetType="DataGridColumnHeader">
                                                            <Setter Property="HorizontalContentAlignment" Value="Center" />
                                                            <Setter Property="Foreground" Value="#252E39" />
                                                            <Setter Property="FontSize" Value="14" />
                                                            <Setter Property="FontWeight" Value="Medium" />
                                                            <Setter Property="FontFamily" Value="Calibri" />
                                                        </Style>
                                                    </DataGridComboBoxColumn.HeaderStyle>
                                                    <DataGridComboBoxColumn.ElementStyle >
                                                        <Style TargetType="ComboBox">
                                                            <Setter Property="ItemsSource" Value="{Binding Path=ListofSupplierServiceType, Mode=OneWay}" />
                                                            <Setter Property="IsSynchronizedWithCurrentItem" Value="False"/>
                                                            <Setter Property="HorizontalAlignment" Value="Left" />
                                                            <Setter Property="HorizontalContentAlignment" Value="Left" />
                                                            <Setter Property="VerticalAlignment" Value="Center" />
                                                            <Setter Property="FontFamily" Value="Calibri" />
                                                            <Setter Property="FontSize" Value="14" />
                                                            <Setter Property="Padding" Value="10 0 0 0" />
                                                        </Style>
                                                    </DataGridComboBoxColumn.ElementStyle>
                                                    <DataGridComboBoxColumn.EditingElementStyle>
                                                        <Style TargetType="ComboBox">
                                                            <Setter Property="ItemsSource" Value="{Binding Path=ListofSupplierServiceType, Mode=OneWay}"/>
                                                            <Setter Property="IsDropDownOpen" Value="True" />
                                                            <Setter Property="HorizontalAlignment" Value="Left" />
                                                            <Setter Property="HorizontalContentAlignment" Value="Left" />
                                                            <Setter Property="VerticalAlignment" Value="Center" />
                                                            <Setter Property="FontFamily" Value="Calibri" />
                                                            <Setter Property="Padding" Value="10 0 0 0" />
                                                        </Style>
                                                    </DataGridComboBoxColumn.EditingElementStyle>

                                                </DataGridComboBoxColumn>


                                                <DataGridComboBoxColumn Header="Currency" x:Name="CmbCurrencyDetails"  DisplayMemberPath="."
                                             SelectedItemBinding="{Binding SelectedItemCurrency, UpdateSourceTrigger=PropertyChanged}" Width="210" Visibility="Hidden">
                                                    <DataGridComboBoxColumn.HeaderStyle>
                                                        <Style TargetType="DataGridColumnHeader">
                                                            <Setter Property="HorizontalContentAlignment" Value="Center" />
                                                            <Setter Property="Foreground" Value="#252E39" />
                                                            <Setter Property="FontSize" Value="14" />
                                                            <Setter Property="FontWeight" Value="Medium" />
                                                            <Setter Property="FontFamily" Value="Calibri" />
                                                        </Style>
                                                    </DataGridComboBoxColumn.HeaderStyle>
                                                    <DataGridComboBoxColumn.ElementStyle >
                                                        <Style TargetType="ComboBox">
                                                            <Setter Property="ItemsSource" Value="{Binding Path=ListofCurrency, Mode=OneWay}" />
                                                            <Setter Property="IsSynchronizedWithCurrentItem" Value="False"/>
                                                            <Setter Property="HorizontalAlignment" Value="Left" />
                                                            <Setter Property="HorizontalContentAlignment" Value="Left" />
                                                            <Setter Property="VerticalAlignment" Value="Center" />
                                                            <Setter Property="FontFamily" Value="Calibri" />
                                                            <Setter Property="FontSize" Value="14" />
                                                            <Setter Property="Padding" Value="5 0 0 0" />
                                                        </Style>
                                                    </DataGridComboBoxColumn.ElementStyle>
                                                    <DataGridComboBoxColumn.EditingElementStyle>
                                                        <Style TargetType="ComboBox">
                                                            <Setter Property="ItemsSource" Value="{Binding Path=ListofCurrency, Mode=OneWay}"/>
                                                            <Setter Property="IsDropDownOpen" Value="True" />
                                                            <Setter Property="HorizontalAlignment" Value="Left" />
                                                            <Setter Property="HorizontalContentAlignment" Value="Left" />
                                                            <Setter Property="VerticalAlignment" Value="Center" />
                                                            <Setter Property="FontFamily" Value="Calibri" />
                                                            <Setter Property="Padding" Value="5 0 0 0" />
                                                        </Style>
                                                    </DataGridComboBoxColumn.EditingElementStyle>

                                                </DataGridComboBoxColumn>



                                                <DataGridComboBoxColumn Header="Charges" x:Name="CmbChargesgroupinfo"  DisplayMemberPath="."
                                             SelectedItemBinding="{Binding SelectedItemGroupInfo, UpdateSourceTrigger=PropertyChanged}" Width="210" Visibility="Hidden">
                                                    <DataGridComboBoxColumn.HeaderStyle>
                                                        <Style TargetType="DataGridColumnHeader">
                                                            <Setter Property="HorizontalContentAlignment" Value="Center" />
                                                            <Setter Property="Foreground" Value="#252E39" />
                                                            <Setter Property="FontSize" Value="14" />
                                                            <Setter Property="FontWeight" Value="Medium" />
                                                            <Setter Property="FontFamily" Value="Calibri" />
                                                        </Style>
                                                    </DataGridComboBoxColumn.HeaderStyle>
                                                    <DataGridComboBoxColumn.ElementStyle >
                                                        <Style TargetType="ComboBox">
                                                            <Setter Property="ItemsSource" Value="{Binding Path=ListofGroupinfo, Mode=OneWay}" />
                                                            <Setter Property="IsSynchronizedWithCurrentItem" Value="False"/>
                                                            <Setter Property="HorizontalAlignment" Value="Center" />
                                                            <Setter Property="HorizontalContentAlignment" Value="Left" />
                                                            <Setter Property="VerticalAlignment" Value="Center" />
                                                            <Setter Property="FontFamily" Value="Calibri" />
                                                            <Setter Property="FontSize" Value="14" />
                                                        </Style>
                                                    </DataGridComboBoxColumn.ElementStyle>
                                                    <DataGridComboBoxColumn.EditingElementStyle>
                                                        <Style TargetType="ComboBox">
                                                            <Setter Property="ItemsSource" Value="{Binding Path=ListofGroupinfo, Mode=OneWay}"/>
