                                        <Button.Background>
                                            <SolidColorBrush Color="#FF579F00" Opacity="0"/>
                                        </Button.Background>
                                        <Button.Resources>
                                            <Style TargetType="{x:Type Border}">
                                                <Setter Property="CornerRadius" Value="1"/>
                                            </Style>
                                        </Button.Resources>
                                    </Button>

                                </Grid>
                            </StackPanel>
                        </StackPanel>
                    </TabItem>

                </TabControl>
            </TabItem>
        </TabControl>

        <TextBox x:Name="hdnSupplierid" Visibility="Hidden" Grid.ColumnSpan="2"></TextBox>
        <TextBox x:Name="hdnSupplierAutoId" Visibility="Hidden" Grid.ColumnSpan="2"></TextBox>
    </Grid>
</Window>
