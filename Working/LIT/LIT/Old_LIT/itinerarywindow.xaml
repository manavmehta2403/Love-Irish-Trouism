                                <TabItem Header="Communications" x:Name="Communication" Style="{StaticResource TabItemWhite}" FontFamily="Calibri" FontSize="16" MinWidth="131" Height="40" Background="#fff" Cursor="Hand">
                                    <Border Background="White" BorderThickness="0,0,0,0" VerticalAlignment="Stretch"  Margin="0,0,0,0" Padding="0">
                                        <Grid>

                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="*"/>
                                            </Grid.ColumnDefinitions>
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="auto"/>
                                                <RowDefinition Height="*" />
                                            </Grid.RowDefinitions>
                                            <Border Background="#f4f4f4" Grid.Column="0" Grid.Row="0" BorderThickness="1" BorderBrush="#e2e3e3" Margin="10">
                                                <Grid>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="60"/>
                                                        <ColumnDefinition Width="200"/>
                                                        <ColumnDefinition Width="150"/>
                                                        <ColumnDefinition Width="150"/>
                                                    </Grid.ColumnDefinitions>
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="*" />
                                                    </Grid.RowDefinitions>
                                                   

                                                    <StackPanel Grid.Column="0" Grid.Row="0" HorizontalAlignment="Stretch" VerticalAlignment="Center" Margin="10,10,10,10">
                                                        <TextBlock Text="Type" FontFamily="Calibri" TextAlignment="Center" FontSize="14"  Foreground="#FF53555E" Padding="0,0,0,5" TextTrimming="CharacterEllipsis"></TextBlock>

                                                    </StackPanel>

                                                    <StackPanel Grid.Column="1" Grid.Row="0" HorizontalAlignment="Stretch" VerticalAlignment="Center" Margin="0,10,10,10">
                                                        <ComboBox SelectedItem="{Binding PdfType}" FontFamily="Calibri"  FontSize="14"  Foreground="#FF53555E" Cursor="Hand"  Padding="5" VerticalAlignment="Center">
                                                            <ComboBoxItem Content = "All" />
                                                            <ComboBoxItem Content = "Quotation" />
                                                            <ComboBoxItem Content= "Deposit Confirmation" />
                                                            <ComboBoxItem Content= "Full Payment Confirmation" />
                                                        </ComboBox>
                                                    </StackPanel>
                                                    <StackPanel Grid.Column="2" Grid.Row="0" HorizontalAlignment="Stretch" VerticalAlignment="Center" Margin="5">
                                                        <Button Cursor="Hand" Command="{Binding EmailLogsSettingCommand.SearchCommand}" Style="{StaticResource blankBlue}" Content="Search" Background="#252e39" Foreground="White" FontSize="16" Height="35"  HorizontalAlignment="Stretch" />
                                                    </StackPanel>
                                                    <StackPanel Grid.Column="3" Grid.Row="0" HorizontalAlignment="Stretch" VerticalAlignment="Center" Margin="5">
                                                        <Button Cursor="Hand" Command="{Binding EmailLogsSettingCommand.RetrieveCommand}" Style="{StaticResource blankBlue}" Content="View All" Background="#579f00" Foreground="White" FontSize="16" Height="35"  HorizontalAlignment="Stretch" />
                                                    </StackPanel>

                                                 


                                                </Grid>
                                            </Border>

                                            <Border Background="#fff" Grid.Column="0" Grid.Row="1" BorderThickness="0" BorderBrush="Transparent" Margin="5">
                                                <Grid>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="*"/>
                                                    </Grid.ColumnDefinitions>
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="*" />
                                                    </Grid.RowDefinitions>
                                                  

                                                    <StackPanel Grid.Row="0" Grid.Column="0">
                                                        <DataGrid HorizontalGridLinesBrush="#D8D9D9" 
                                                                  ItemsSource="{Binding EmailLogsSettingCollection}"
                                                                  SelectedItem="{Binding SelecteEmailSetting}"
                                                                  VerticalGridLinesBrush="#D8D9D9" 
                           BorderBrush="#D8D9D9" 
                           BorderThickness="0,1,0,0" 
                           VerticalAlignment="Top" 
                           CanUserAddRows="False" 
                           AutoGenerateColumns="False" 
                           HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" 
                           Background="White"
                           behavior:NewSingleClickDataGridBehavior.EnableSingleClickEditing="True" >
                                                            <DataGrid.Columns>


                                                                <DataGridTemplateColumn Header="Sent Date" MinWidth="115" Width="*">
                                                                    <DataGridTemplateColumn.HeaderStyle>
                                                                        <Style TargetType="{x:Type DataGridColumnHeader}">
                                                                            <Setter Property="HorizontalContentAlignment" Value="Center" />
                                                                            <Setter Property="Foreground" Value="#252E39" />
                                                                            <Setter Property="FontFamily" Value="Calibri"/>
                                                                            <Setter Property="FontSize" Value="16"/>
                                                                        </Style>
                                                                    </DataGridTemplateColumn.HeaderStyle>
                                                                    <DataGridTemplateColumn.CellTemplate>
                                                                        <DataTemplate>
                                                                            <TextBlock VerticalAlignment="Center" HorizontalAlignment="Center" TextAlignment="Center" Padding="7,0,7,0" FontSize="14" FontFamily="Calibri" >
                                                                                <TextBlock.Text>
                                                                                    <Binding Path="SentDate" StringFormat="dd/MM/yyyy HH:mm" UpdateSourceTrigger="PropertyChanged"/>
                                                                                </TextBlock.Text>
                                                                            </TextBlock>
                                                                        </DataTemplate>
                                                                    </DataGridTemplateColumn.CellTemplate>
                                                                </DataGridTemplateColumn>

                                                                <DataGridTemplateColumn Header="Type" MinWidth="100" Width="*">
                                                                    <DataGridTemplateColumn.HeaderStyle>
                                                                        <Style TargetType="{x:Type DataGridColumnHeader}">
                                                                            <Setter Property="HorizontalContentAlignment" Value="Center" />
                                                                            <Setter Property="Foreground" Value="#252E39" />
                                                                            <Setter Property="FontFamily" Value="Calibri"/>
                                                                            <Setter Property="FontSize" Value="16"/>
                                                                        </Style>
                                                                    </DataGridTemplateColumn.HeaderStyle>
                                                                    <DataGridTemplateColumn.CellTemplate>
                                                                        <DataTemplate>
                                                                            <TextBlock Text="{Binding Type, UpdateSourceTrigger=PropertyChanged }" VerticalAlignment="Center" HorizontalAlignment="Stretch" TextAlignment="Left" Padding="7,0,7,0" FontSize="14" FontFamily="Calibri" />
                                                                        </DataTemplate>
                                                                    </DataGridTemplateColumn.CellTemplate>
                                                                </DataGridTemplateColumn>



                                                                <DataGridTemplateColumn Header="Sender Email" MinWidth="100" Width="*">
                                                                    <DataGridTemplateColumn.HeaderStyle>
                                                                        <Style TargetType="{x:Type DataGridColumnHeader}">
                                                                            <Setter Property="HorizontalContentAlignment" Value="Center" />
                                                                            <Setter Property="Foreground" Value="#252E39" />
                                                                            <Setter Property="FontFamily" Value="Calibri"/>
                                                                            <Setter Property="FontSize" Value="16"/>
                                                                        </Style>
                                                                    </DataGridTemplateColumn.HeaderStyle>
                                                                    <DataGridTemplateColumn.CellTemplate>
                                                                        <DataTemplate>
                                                                            <TextBlock Text="{Binding FromAddress , UpdateSourceTrigger=PropertyChanged }" VerticalAlignment="Center" HorizontalAlignment="Stretch" TextAlignment="Left" Padding="7,0,7,0" FontSize="14" FontFamily="Calibri" />
                                                                        </DataTemplate>
                                                                    </DataGridTemplateColumn.CellTemplate>
                                                                </DataGridTemplateColumn>
                                                                

                                                                <DataGridTemplateColumn Header="Recipient Email" MinWidth="100" Width="*">
                                                                    <DataGridTemplateColumn.HeaderStyle>
                                                                        <Style TargetType="{x:Type DataGridColumnHeader}">
                                                                            <Setter Property="HorizontalContentAlignment" Value="Center" />
                                                                            <Setter Property="Foreground" Value="#252E39" />
                                                                            <Setter Property="FontFamily" Value="Calibri"/>
                                                                            <Setter Property="FontSize" Value="16"/>
                                                                        </Style>
                                                                    </DataGridTemplateColumn.HeaderStyle>
                                                                    <DataGridTemplateColumn.CellTemplate>
                                                                        <DataTemplate>
                                                                            <TextBlock Text="{Binding RecipientEmail, UpdateSourceTrigger=PropertyChanged}" VerticalAlignment="Center" HorizontalAlignment="Stretch" TextAlignment="Left" Padding="7,0,7,0" FontSize="14" FontFamily="Calibri"/>
                                                                        </DataTemplate>
                                                                    </DataGridTemplateColumn.CellTemplate>
                                                                </DataGridTemplateColumn>


                                                                <DataGridTemplateColumn Header="Recipient" MinWidth="100" Width="*">
                                                                    <DataGridTemplateColumn.HeaderStyle>
                                                                        <Style TargetType="{x:Type DataGridColumnHeader}">
                                                                            <Setter Property="HorizontalContentAlignment" Value="Center" />
                                                                            <Setter Property="Foreground" Value="#252E39" />
                                                                            <Setter Property="FontFamily" Value="Calibri"/>
                                                                            <Setter Property="FontSize" Value="16"/>
                                                                        </Style>
                                                                    </DataGridTemplateColumn.HeaderStyle>
                                                                    <DataGridTemplateColumn.CellTemplate>
                                                                        <DataTemplate>
                                                                            <TextBlock Text="{Binding Recipient, UpdateSourceTrigger=PropertyChanged}" VerticalAlignment="Center" HorizontalAlignment="Stretch" TextAlignment="Left" Padding="7,0,7,0" FontSize="14" FontFamily="Calibri"/>
                                                                        </DataTemplate>
                                                                    </DataGridTemplateColumn.CellTemplate>
                                                                </DataGridTemplateColumn>



                                                                <DataGridTemplateColumn Header="Subject" MinWidth="200" Width="*">
                                                                    <DataGridTemplateColumn.HeaderStyle>
                                                                        <Style TargetType="{x:Type DataGridColumnHeader}">
                                                                            <Setter Property="HorizontalContentAlignment" Value="Center" />
                                                                            <Setter Property="Foreground" Value="#252E39" />
                                                                            <Setter Property="FontFamily" Value="Calibri"/>
                                                                            <Setter Property="FontSize" Value="16"/>
                                                                        </Style>
                                                                    </DataGridTemplateColumn.HeaderStyle>
                                                                    <DataGridTemplateColumn.CellTemplate>
                                                                        <DataTemplate>
                                                                            <TextBlock Text="{Binding EmailSubject, UpdateSourceTrigger=PropertyChanged}" VerticalAlignment="Center" HorizontalAlignment="Left" FontSize="14" FontFamily="Calibri" Padding="5,5"/>
                                                                        </DataTemplate>
                                                                    </DataGridTemplateColumn.CellTemplate>
                                                                </DataGridTemplateColumn>



                                                                <DataGridTemplateColumn Header="Email Content" MinWidth="100" Width="*">
                                                                    <DataGridTemplateColumn.HeaderStyle>
                                                                        <Style TargetType="{x:Type DataGridColumnHeader}">
                                                                            <Setter Property="HorizontalContentAlignment" Value="Center" />
                                                                            <Setter Property="Foreground" Value="#252E39" />
                                                                            <Setter Property="FontFamily" Value="Calibri"/>
                                                                            <Setter Property="FontSize" Value="16"/>
                                                                        </Style>
                                                                    </DataGridTemplateColumn.HeaderStyle>
                                                                    <DataGridTemplateColumn.CellTemplate>
                                                                        <DataTemplate>
                                                                            <Button Style="{StaticResource btnBlue}"
                                                                                    Click="Button_Click_1"
                                                                                    VerticalAlignment="Center" HorizontalAlignment="Center" VerticalContentAlignment="Center" Background="White" HorizontalContentAlignment="Center">
                                                                                <Image Source="/LIT.Core;component/Media/Images/linkout.png" Width="13" Height="auto" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                                                            </Button>
                                                                            
                                                                            <!--<TextBlock Text="{Binding BookingId, UpdateSourceTrigger=PropertyChanged}" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="14" FontFamily="Calibri"/>-->
                                                                        </DataTemplate>
                                                                    </DataGridTemplateColumn.CellTemplate>
                                                                </DataGridTemplateColumn>


                                                                <DataGridTemplateColumn Header="Attachment" MinWidth="100" Width="*">
                                                                    <DataGridTemplateColumn.HeaderStyle>
                                                                        <Style TargetType="{x:Type DataGridColumnHeader}">
                                                                            <Setter Property="HorizontalContentAlignment" Value="Center" />
                                                                            <Setter Property="Foreground" Value="#252E39" />
                                                                            <Setter Property="FontFamily" Value="Calibri"/>
                                                                            <Setter Property="FontSize" Value="16"/>
                                                                        </Style>
                                                                    </DataGridTemplateColumn.HeaderStyle>
                                                                    <DataGridTemplateColumn.CellTemplate>
                                                                        <DataTemplate>
                                                                            <Button Style="{StaticResource btnBlue}"
                                                                                    Click="Button_Click"
                                                                                    VerticalAlignment="Center" HorizontalAlignment="Center" VerticalContentAlignment="Center" HorizontalContentAlignment="Center">
                                                                                <Image Source="/LIT.Core;component/Media/Images/icon_pdf.png" Width="13" Height="auto" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                                                            </Button>

                                                                            <!--<TextBlock Text="{Binding BookingId, UpdateSourceTrigger=PropertyChanged}" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="14" FontFamily="Calibri"/>-->
                                                                        </DataTemplate>
                                                                    </DataGridTemplateColumn.CellTemplate>
                                                                </DataGridTemplateColumn>



                                                                <DataGridTemplateColumn Header="Save as Word" MinWidth="100" Width="*">
                                                                    <DataGridTemplateColumn.HeaderStyle>
                                                                        <Style TargetType="{x:Type DataGridColumnHeader}">
                                                                            <Setter Property="HorizontalContentAlignment" Value="Center" />
                                                                            <Setter Property="Foreground" Value="#252E39" />
                                                                            <Setter Property="FontFamily" Value="Calibri"/>
                                                                            <Setter Property="FontSize" Value="16"/>
                                                                        </Style>
                                                                    </DataGridTemplateColumn.HeaderStyle>
                                                                    <DataGridTemplateColumn.CellTemplate>
                                                                        <DataTemplate>
                                                                            <Button Style="{StaticResource btnBlue}" x:Name="btnsaveworddoc"  VerticalAlignment="Center" HorizontalAlignment="Center" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Click="btnsaveworddoc_Click">
                                                                                <Image Source="/LIT.Core;component/Media/Images/words.png" Width="13" Height="auto" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                                                            </Button>
                                                                            <!--<TextBlock Text="{Binding BookingId, UpdateSourceTrigger=PropertyChanged}" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="14" FontFamily="Calibri"/>-->
                                                                        </DataTemplate>
                                                                    </DataGridTemplateColumn.CellTemplate>
                                                                </DataGridTemplateColumn>

                                                            </DataGrid.Columns>
                                                        </DataGrid>
                                                    </StackPanel>

                                                </Grid>
                                            </Border>

                                        </Grid>
                                    </Border>
                                </TabItem>
                            
                            </TabControl>
                        </StackPanel>
                    </TabItem>
                    <TabItem x:Name="Commentab" Header="Comments" Cursor="Hand" FontSize="16" Width="131"  Height="48"  Style="{StaticResource TabItemBase}"  Margin="-2,0,2,0" VerticalAlignment="Top" AllowDrop="True" >
                        <tabcntrl:ItineraryCommentsTab x:Name="ItineraryCommentsTab" Margin="10" Padding="5"  Background="#f5f5f5" >

                        </tabcntrl:ItineraryCommentsTab>
                    </TabItem>
                </TabControl>

            </StackPanel>
        </Grid>

    </Grid>
</UserControl>
