<UserControl x:Class="LIT.Old_LIT.ItineraryWindow"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:LIT.Old_LIT"
      xmlns:resGridex="clr-namespace:LIT.Core.Controls.GridExtensions;assembly=LIT.Core"      
      xmlns:tabcntrl="clr-namespace:LIT.Modules.TabControl.Views;assembly=LIT.Modules.TabControl"
      xmlns:behavior="clr-namespace:LIT.Core.Behaviors;assembly=LIT.Core"
      xmlns:viewModel="clr-namespace:LIT.ViewModels"
      xmlns:control = "clr-namespace:LIT" 
      mc:Ignorable="d" 
       
      Height="auto" Width="auto" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" x:Name="controlTabsFill" Margin="0,-1,0,0">

    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/LIT.Core;component/Styles/PasswordBoxStyles.xaml"/>
                <ResourceDictionary Source="/LIT.Core;component/Styles/TextBoxStyles.xaml"/>
                <ResourceDictionary Source="/LIT.Core;component/Styles/ButtonStyles.xaml"/>
                <ResourceDictionary Source="/LIT.Core;component/Styles/TabItemStyles.xaml"/>
                <ResourceDictionary Source="/LIT.Core;component/Styles/MenuItemStyles.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid>
        <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*" />
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="50" />
                <RowDefinition Height="*" />
            </Grid.RowDefinitions>
            <StackPanel Grid.Column="0" Grid.Row="0">
                <Grid VerticalAlignment="Top" Background="#579f00">
                    <Grid.ColumnDefinitions>
                        <!--<ColumnDefinition Width="297.83" />-->
                        <ColumnDefinition />
                        <ColumnDefinition MaxWidth="145" />
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="50"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>

                    <DockPanel  Grid.Row="0" Grid.Column="0" HorizontalAlignment="Left"  VerticalAlignment="Center" Margin="10,0,0,0">

                        <TextBlock FontFamily="Calibri" FontSize="18" Foreground="White" FontWeight="SemiBold" x:Name="TxtblItinNamewithpath" ></TextBlock>

                    </DockPanel>

                    <DockPanel Grid.Row="0" Grid.Column="1" Background="Transparent" HorizontalAlignment="Center" Width="150" Visibility="Collapsed">
                        <Button Style="{StaticResource btnBlue}" Name="BtnSave" Background="Transparent" Click="BtnSave_Click" FontFamily="Calibri" Height="30" FontSize="16" BorderBrush="Transparent" Foreground="#FF579F00" FontWeight="Bold" MaxWidth="79" Width="79" Cursor="Hand" MouseEnter="mouseEnterSave" MouseLeave="museLeaveOption" Visibility="Collapsed" >

                            <StackPanel Orientation="Horizontal">
                                <Image Source="/LIT.Core;component/Media/Images/save.png" Width="16" VerticalAlignment="Center"></Image>
                                <Label Foreground="#FF579F00" FontSize="16" VerticalAlignment="Center">Save</Label>
                            </StackPanel>
                        </Button>

                        <Button  Style="{StaticResource btnBlue}" Click="BtnSaveandclose_Click" Name="BtnSaveandclose" FontFamily="Calibri" Height="30" Background="Transparent" FontSize="16" BorderBrush="#FF579F00" Foreground="#FF579F00" FontWeight="Bold"  MaxWidth="120" Width="120" BorderThickness="2,0,0,0" Cursor="Hand" MouseEnter="mouseEnterSaveClose" MouseLeave="museLeaveOptionSaveClose" HorizontalContentAlignment="Center" Margin="15,0,15,0" HorizontalAlignment="Center" Visibility="Collapsed" >
                            <StackPanel Orientation="Horizontal">
                                <Image Source="/LIT.Core;component/Media/Images/save.png" Width="16" VerticalAlignment="Center"></Image>
                                <Label Foreground="#FF579F00" FontSize="16" VerticalAlignment="Center">Save &amp; Close</Label>
                            </StackPanel>
                        </Button>

                    </DockPanel>


                </Grid>
            </StackPanel>

            <StackPanel Grid.Column="0" Grid.Row="1" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" >

                <TabControl SelectionChanged="TabControl_SelectionChanged" Width="auto"  Height="auto"  MinWidth="{Binding ElementName=Navigationframe, Path=ActualWidth}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Background="#4d4d4d" BorderBrush="#FF4D4D4D" BorderThickness="1,1,1,1"  >

                    <TabItem Header="Itinerary" x:Name="ItineraryCore" Cursor="Hand" FontSize="16" Width="131"  Height="48"  Style="{StaticResource TabItemBase}"  Margin="-4,0,0,0" >
                        <Border Background="#f5f5f5" HorizontalAlignment="Stretch" BorderBrush="#b9b9b9" BorderThickness="1,1,1,1" VerticalAlignment="Stretch" CornerRadius="10" Margin="10,10,10,10" Padding="10, 10" Width="auto" Height="auto">
                            <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition MinWidth="162" Width="*"/>
                                    <ColumnDefinition MinWidth="162" Width="*"/>
                                    <ColumnDefinition MinWidth="162" Width="*"/>
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition MinHeight="38" />
                                    <RowDefinition MinHeight="50" />
                                    <RowDefinition MinHeight="50" />
                                    <RowDefinition MinHeight="50" />
                                    <RowDefinition MinHeight="50" />
                                    <RowDefinition MinHeight="38" />
                                    <RowDefinition MinHeight="50" />
                                    <RowDefinition MinHeight="50" />
                                    <RowDefinition MinHeight="38" />
                                    <RowDefinition MinHeight="50" />
                                    <RowDefinition MinHeight="50" />
                                    <RowDefinition Height="*" />
                                </Grid.RowDefinitions>

                                <StackPanel Orientation="Vertical" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Height="24"  Grid.Column="0" Grid.Row="0" Margin="10,0,10,0">
                                    <Label Content="Core Information" FontFamily="Calibri" Width="auto" Height="24" FontSize="20" Foreground="#FF25304C" Padding="0,0,0,0" FontWeight="Bold"  />
                                </StackPanel>

                                <StackPanel Orientation="Vertical" HorizontalAlignment="Stretch" x:Name="TxtItineraryName_stk"  VerticalAlignment="Stretch" Height="50"  Grid.Column="0" Grid.Row="1" Margin="10,0,10,0">
                                    <Label Content="Itinerary Name" FontFamily="Calibri" Width="auto" Height="22" FontSize="14"  Foreground="#FF53555E" Padding="0,0,0,0"  />
                                    <TextBox Template="{StaticResource TextBoxBaseControlTemplate}" TabIndex="1" x:Name="TxtItineraryName" FontFamily="Calibri" Height="24" FontSize="14" VerticalContentAlignment="Center" VerticalAlignment="Center" Padding="5,0,5,0" BorderBrush="#FFA1A1A1" />
                                </StackPanel>

                                <StackPanel Orientation="Vertical"  HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Height="50" Grid.Column="1" Grid.Row="1" Margin="10,0,10,0">
                                    <Label Content="Itinerary ID" FontFamily="Calibri" Width="auto" Height="22" FontSize="14" Foreground="#FF53555E" Padding="0,0,0,0"  />
                                    <TextBox Template="{StaticResource TextBoxBaseControlTemplate}"  TabIndex="2" x:Name="TxtItineraryID" IsEnabled="False" FontFamily="Calibri" Width="auto" VerticalContentAlignment="Center"  Height="24" FontSize="14" Padding="5,0,5,0" Background="#FFEAEAEA" BorderBrush="#FFA1A1A1" />

                                </StackPanel>

                                <StackPanel Orientation="Vertical"  HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Height="50" Width="auto" Grid.Column="2" Grid.Row="1" Margin="10,0,10,0">
                                    <Label Content="Date Created" FontFamily="Calibri" Width="auto" Height="22" FontSize="14"  Foreground="#FF53555E" Padding="0,0,0,0"  />
                                    <DatePicker x:Name="TxtDateCreated"  TabIndex="3" FontFamily="Calibri" Width="auto" Height="24" FontSize="14" BorderBrush="#FFA1A1A1" VerticalContentAlignment="Center" HorizontalContentAlignment="Stretch"   >
                                        <DatePicker.Background>
                                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                                <GradientStop Color="White"/>
                                                <GradientStop Color="White" Offset="1"/>
                                            </LinearGradientBrush>
                                        </DatePicker.Background>
                                    </DatePicker>
                                </StackPanel>

                                <StackPanel Orientation="Vertical"  HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Height="50"  Grid.Column="0" Grid.Row="2" Margin="10,0,10,0">
                                    <Label Content="Display Name" FontFamily="Calibri" Width="auto" Height="22" FontSize="14"  Foreground="#FF53555E" Padding="0,0,0,0"  />
                                    <TextBox Template="{StaticResource TextBoxBaseControlTemplate}"  TabIndex="4" x:Name="TxtDisplayName" FontFamily="Calibri" Width="auto" Height="24" FontSize="14" VerticalContentAlignment="Center" Padding="5,0,5,0" BorderBrush="#FFA1A1A1" />
                                </StackPanel>

                                <StackPanel Orientation="Vertical"  HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Height="50"  Grid.Column="1" Grid.Row="2" Margin="10,0,10,0">
                                    <Label Content="Email *" FontFamily="Calibri" Width="auto" Height="22" FontSize="14"  Foreground="#FF53555E" Padding="0,0,0,0"  />
                                    <TextBox Template="{StaticResource TextBoxBaseControlTemplate}"  TabIndex="5" x:Name="TxtEmail" FontFamily="Calibri" Width="auto" Height="24" FontSize="14" VerticalContentAlignment="Center" Padding="5,0,5,0" BorderBrush="#FFA1A1A1" />
                                </StackPanel>

                                
                                <StackPanel Orientation="Vertical"  HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Height="50"  Grid.Column="0" Grid.Row="3" Margin="10,0,10,0">
                                    <Label Content="Client ID" FontFamily="Calibri" Width="auto" Height="22" FontSize="14"  Foreground="#FF53555E" Padding="0,0,0,0"  />
                                    <TextBox Template="{StaticResource TextBoxBaseControlTemplate}"  TabIndex="6" x:Name="TxtCustomerID" FontFamily="Calibri" Width="auto" Height="24" FontSize="14" VerticalContentAlignment="Center" Padding="5,0,5,0" BorderBrush="#FFA1A1A1" />
                                </StackPanel>
                                <StackPanel Orientation="Vertical"  HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Height="50"  Grid.Column="1" Grid.Row="3" Margin="10,0,10,0">
                                    <Label Content="Phone" FontFamily="Calibri" Width="auto" Height="22" FontSize="14"  Foreground="#FF53555E" Padding="0,0,0,0"  />
                                    <TextBox Template="{StaticResource TextBoxBaseControlTemplate}"  TabIndex="7" x:Name="TxtPhone" FontFamily="Calibri" Width="auto" Height="24" FontSize="14" VerticalContentAlignment="Center" Padding="5,0,5,0" BorderBrush="#FFA1A1A1" />
                                </StackPanel>

                                <StackPanel Orientation="Vertical" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Height="Auto"  Grid.Column="2" Grid.ColumnSpan="2" Grid.Row="2" Grid.RowSpan="2" Margin="10,0,10,0">
                                    <Label Content="Inclusion" FontFamily="Calibri" Width="auto" Height="22" FontSize="14"  Foreground="#FF53555E" Padding="0,0,0,0" DockPanel.Dock="Top" />
                                    <TextBox Template="{StaticResource TextBoxBaseControlTemplate}"   TabIndex="8" x:Name="TxtInclusion" FontFamily="Calibri" Width="auto" Height="72" FontSize="14" VerticalContentAlignment="Top" Padding="5,0,5,0" BorderBrush="#FFA1A1A1" TextWrapping="Wrap" AcceptsReturn="True" HorizontalScrollBarVisibility="Disabled" VerticalScrollBarVisibility="Auto" >

                                    </TextBox>
                                </StackPanel>

                                <StackPanel Orientation="Vertical"  HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Height="50"  Grid.Column="0" Grid.Row="4" Margin="10,0,10,0">
                                    <Label Content="Client First Name *" FontFamily="Calibri" Width="auto" Height="22" FontSize="14"  Foreground="#FF53555E" Padding="0,0,0,0"  />
                                    <TextBox Template="{StaticResource TextBoxBaseControlTemplate}"  TabIndex="9" x:Name="TxtClientFirstName" FontFamily="Calibri" Width="auto" Height="24" FontSize="14" VerticalContentAlignment="Center" Padding="5,0,5,0" BorderBrush="#FFA1A1A1" LostFocus="TxtClientFirstName_LostFocus" />
                                </StackPanel>
                                <StackPanel Orientation="Vertical"  HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Height="50"  Grid.Column="1" Grid.Row="4" Margin="10,0,10,0">
                                    <Label Content="Client Last Name *" FontFamily="Calibri" Width="auto" Height="22" FontSize="14"  Foreground="#FF53555E" Padding="0,0,0,0"  />
                                    <TextBox Template="{StaticResource TextBoxBaseControlTemplate}"  TabIndex="10" x:Name="TxtClientLastname" FontFamily="Calibri" Width="auto" Height="24" FontSize="14" VerticalContentAlignment="Center" Padding="5,0,5,0" BorderBrush="#FFA1A1A1" LostFocus="TxtClientLastname_LostFocus" />
                                </StackPanel>
                                <StackPanel Orientation="Vertical"  HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Height="50"  Grid.Column="2" Grid.Row="4" Margin="10,0,10,0">
                                    <Label Content="Client Display Name" FontFamily="Calibri" Width="auto" Height="22" FontSize="14"  Foreground="#FF53555E" Padding="0,0,0,0"  />
                                    <TextBox Template="{StaticResource TextBoxBaseControlTemplate}" IsReadOnly="True"  TabIndex="0" x:Name="TxtClientdisplayname" FontFamily="Calibri" Width="auto" Height="24" FontSize="14" VerticalContentAlignment="Center" Padding="5,0,5,0" BorderBrush="#FFA1A1A1" />
                                </StackPanel>

                                <StackPanel Orientation="Vertical"  HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Height="24"  Grid.Column="0" Grid.Row="5" Margin="10,0,10,0">
                                    <Label Content="Arrival &amp; Departure Information" FontFamily="Calibri" Width="auto" Height="24" FontSize="20" Foreground="#FF25304C" Padding="0,0,0,0" FontWeight="Bold"  />
                                </StackPanel>

                                <StackPanel Orientation="Vertical"   HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Height="50" Width="auto" Grid.Column="0" Grid.Row="6" Margin="10,0,10,0">
                                    <Label Content="Itinerary Start Date *" FontFamily="Calibri" Width="auto" Height="22" FontSize="14"  Foreground="#FF53555E" Padding="0,0,0,0"  />
                                    <DatePicker x:Name="TxtItinerarystartdt"  TabIndex="12" FontFamily="Calibri" Width="auto" Height="24" FontSize="14" BorderBrush="#FFA1A1A1" VerticalContentAlignment="Center" HorizontalContentAlignment="Stretch"   >
                                        <DatePicker.Background>
                                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                                <GradientStop Color="White"/>
                                                <GradientStop Color="White" Offset="1"/>
                                            </LinearGradientBrush>
                                        </DatePicker.Background>
                                    </DatePicker>
                                </StackPanel>

                                <StackPanel Orientation="Vertical"  HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Height="50"  Grid.Column="1" Grid.Row="6" Margin="10,0,10,0">
                                    <Label Content="Arrival City" FontFamily="Calibri" Width="auto" Height="22" FontSize="14"  Foreground="#FF53555E" Padding="0,0,0,0"  />
                                    <ComboBox x:Name="CmbArrivalCity"  TabIndex="13" FontFamily="Calibri" Width="auto" Height="24" FontSize="14" VerticalContentAlignment="Center" Padding="5,0,5,0" BorderBrush="#FFA1A1A1">
                                        <ComboBox.Background>
                                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0" MappingMode="RelativeToBoundingBox">
                                                <GradientStop Color="White"/>
                                                <GradientStop Color="White" Offset="1"/>
                                            </LinearGradientBrush>
                                        </ComboBox.Background>
                                    </ComboBox>
                                </StackPanel>

                                <StackPanel Orientation="Vertical"  HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Height="50"  Grid.Column="2" Grid.Row="6" Margin="10,0,10,0">
                                    <Label Content="Arrival Flight" FontFamily="Calibri" Width="auto" Height="22" FontSize="14"  Foreground="#FF53555E" Padding="0,0,0,0"  />
                                    <TextBox TabIndex="14" Template="{StaticResource TextBoxBaseControlTemplate}" x:Name="TxtArrivalFlight" FontFamily="Calibri" Width="auto" Height="24" FontSize="14" VerticalContentAlignment="Center" Padding="5,0,5,0" BorderBrush="#FFA1A1A1" />
                                </StackPanel>

                                <StackPanel Orientation="Vertical" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Height="50" Width="auto" Grid.Column="0" Grid.Row="7" Margin="10,0,10,0">
                                    <Label Content="Itinerary End Date *" FontFamily="Calibri" Width="auto" Height="22" FontSize="14"  Foreground="#FF53555E" Padding="0,0,0,0"  />
                                    <DatePicker TabIndex="15" x:Name="TxtItineraryEndDt" FontFamily="Calibri" Width="auto" Height="24" FontSize="14" BorderBrush="#FFA1A1A1" VerticalContentAlignment="Center" HorizontalContentAlignment="Stretch"   >
                                        <DatePicker.Background>
                                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                                <GradientStop Color="White"/>
                                                <GradientStop Color="White" Offset="1"/>
                                            </LinearGradientBrush>
                                        </DatePicker.Background>
                                    </DatePicker>
                                </StackPanel>

                                <StackPanel Orientation="Vertical"  HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Height="50"  Grid.Column="1" Grid.Row="7" Margin="10,0,10,0">
                                    <Label Content="Departure City" FontFamily="Calibri" Width="auto" Height="22" FontSize="14"  Foreground="#FF53555E" Padding="0,0,0,0"  />
                                    <ComboBox TabIndex="16" x:Name="CmbDepartureCity" FontFamily="Calibri" Width="auto" Height="24" FontSize="14" VerticalContentAlignment="Center" Padding="5,0,5,0"  BorderBrush="#FFA1A1A1">
                                    </ComboBox>
                                </StackPanel>

                                <StackPanel Orientation="Vertical" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Height="50"  Grid.Column="2" Grid.Row="7" Margin="10,0,10,0">
                                    <Label Content="Departure Flight" FontFamily="Calibri" Width="auto" Height="22" FontSize="14"  Foreground="#FF53555E" Padding="0,0,0,0"  />
                                    <TextBox  TabIndex="17" Template="{StaticResource TextBoxBaseControlTemplate}" x:Name="TxtDepartureFlight" FontFamily="Calibri" Width="auto" Height="24" FontSize="14" VerticalContentAlignment="Center" Padding="5,0,5,0" BorderBrush="#FFA1A1A1" />
                                </StackPanel>

                                <StackPanel Orientation="Vertical" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Height="24"  Grid.Column="0" Grid.Row="8" Margin="10,0,10,0">
                                    <Label Content="Agent Information" FontFamily="Calibri" Width="auto" Height="24" FontSize="20" Foreground="#FF25304C" Padding="0,0,0,0" FontWeight="Bold"  />
                                </StackPanel>

                                <StackPanel Orientation="Vertical" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Height="50"  Grid.Column="0" Grid.Row="9" Margin="10,0,10,0">
                                    <Label Content="Agent" FontFamily="Calibri" Width="auto" Height="22" FontSize="14"  Foreground="#FF53555E" Padding="0,0,0,0"  />
                                    <ComboBox x:Name="CmbAgent" FontFamily="Calibri" Width="auto" TabIndex="18" Height="24" FontSize="14" VerticalContentAlignment="Center" Padding="5,0,5,0" BorderBrush="#FFA1A1A1">
                                    </ComboBox>
                                </StackPanel>

                                <StackPanel Orientation="Vertical" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Height="50"  Grid.Column="1" Grid.Row="9" Margin="10,0,10,0">
                                    <Label Content="Agent Assigned To" FontFamily="Calibri" Width="auto" Height="22" FontSize="14"  Foreground="#FF53555E" Padding="0,0,0,0"  />
                                    <ComboBox x:Name="CmbAgentAssignedTo" TabIndex="19" FontFamily="Calibri" Width="auto" Height="24" FontSize="14" VerticalContentAlignment="Center" Padding="5,0,5,0" BorderBrush="#FFA1A1A1">
                                    </ComboBox>
                                </StackPanel>

                                <StackPanel Orientation="Vertical"  HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Height="50"  Grid.Column="2" Grid.Row="9" Margin="10,0,10,0">
                                    <Label Content="Entered By" FontFamily="Calibri" Width="auto" Height="22" FontSize="14"  Foreground="#FF53555E" Padding="0,0,0,0"  />
                                    <TextBox Template="{StaticResource TextBoxBaseControlTemplate}" TabIndex="0" x:Name="TxtEnteredBy" FontFamily="Calibri" Width="auto" Height="24" FontSize="14" VerticalContentAlignment="Center" Padding="5,0,5,0" BorderBrush="#FFA1A1A1" />
                                </StackPanel>

                                <StackPanel Orientation="Vertical"  HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Height="50"  Grid.Column="0" Grid.Row="10" Margin="10,0,10,0">
                                    <Label Content="Status" FontFamily="Calibri" Width="auto" Height="22" FontSize="14"  Foreground="#FF53555E" Padding="0,0,0,0"  />
                                    <ComboBox x:Name="CmbStatus" TabIndex="21" FontFamily="Calibri" Width="auto" Height="24" FontSize="14" VerticalContentAlignment="Center" Padding="5,0,5,0" BorderBrush="#FFA1A1A1">
                                        <!--SelectionChanged="CmbStatus_SelectionChanged"-->
                                    </ComboBox>
                                </StackPanel>

                                <StackPanel Orientation="Vertical"  HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Height="50"  Grid.Column="1" Grid.Row="10" Margin="10,0,10,0">
                                    <Label Content="Source" FontFamily="Calibri" Width="auto" Height="22" FontSize="14"  Foreground="#FF53555E" Padding="0,0,0,0"  />
                                    <ComboBox x:Name="CmbSource" TabIndex="22" FontFamily="Calibri" Width="auto" Height="24" FontSize="14" VerticalContentAlignment="Center" Padding="5,0,5,0"   BorderBrush="#FFA1A1A1">
                                    </ComboBox>
                                </StackPanel>

                                <TextBox x:Name="hdnitineraryid" Visibility="Hidden"></TextBox>
                                <TextBox x:Name="hdnmode" Visibility="Hidden"></TextBox>
                                <TextBox x:Name="TxtItineraryFolderPath" Visibility="Hidden"></TextBox>
                                <TextBox x:Name="hdnitineraryAutono" Visibility="Hidden"></TextBox>
                                <TextBox x:Name="hdnItineraryBookingTotalId" Visibility="Hidden"></TextBox>
                                
                            </Grid>

                        </Border>
                    </TabItem>
                    <TabItem Header="Booking" Cursor="Hand" x:Name="ItinBooking" FontSize="16" Width="131"  Style="{StaticResource TabItemBase}"  Margin="-2,0,2,0" AllowDrop="True" Height="48" VerticalAlignment="Top">

                        <StackPanel Width="auto" Background="#f5f5f5" Margin="10, 10, 10, 10" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">

                            
                            <TabControl Width="auto" MinHeight="{Binding ElementName=Navigationframe, Path=ActualHeight}" MinWidth="{Binding ElementName=Navigationframe, Path=ActualWidth}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" Background="#fff" Margin="10,10,10,10">

                                <TabItem Header="List" Style="{StaticResource TabItemWhite}" FontFamily="Calibri" FontSize="16" Width="131"  Height="40"  Background="#fff"   Margin="-4,0,0,0" Cursor="Hand" >
                                    <Border Background="White" BorderThickness="0,0,0,0" VerticalAlignment="Top"  Margin="00,0,0,0" Padding="0">
                                        <StackPanel>
                                            <Grid Margin="0,10,0,10">
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="120"/>
                                                    <ColumnDefinition Width="auto" MinWidth="200"/>
                                                    <ColumnDefinition />
                                                    <ColumnDefinition Width="180"/>
                                                    <ColumnDefinition Width="180"/>
                                                    <ColumnDefinition Width="180"/>
                                                    <ColumnDefinition Width="80"/>
                                                    <ColumnDefinition Width="Auto" />
                                                    <ColumnDefinition Width="80"/>
                                                </Grid.ColumnDefinitions>
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="40" />
                                                </Grid.RowDefinitions>
                                               
                                                <!-- Addon Supplier Payment Button -->
                                                <DockPanel Grid.Row="0" Grid.Column="2" Background="Transparent" HorizontalAlignment="Left" Width="auto" Visibility="Collapsed">
                                                    <Button Name="SupplierPaymentOption" Style="{StaticResource btnBlue}" Background="Transparent" FontFamily="Calibri" Height="30"  FontSize="14" BorderBrush="Transparent" Foreground="#FF579F00" FontWeight="Bold"   BorderThickness="2,0,0,0" Cursor="Hand"  HorizontalContentAlignment="Center"  HorizontalAlignment="Center" Click="SupplierPaymentOption_Click">
                                                        <StackPanel Orientation="Horizontal">
                                                            <Image Source="/LIT.Core;component/Media/Images/money-note.png" Width="24" VerticalAlignment="Center"></Image>
                                                            <Label Foreground="#FF579F00" FontSize="16" VerticalAlignment="Center">Suppliers Payment</Label>
                                                        </StackPanel>
                                                    </Button>
                                                </DockPanel>
                                                
                                                <!-- /Addon Supplier Payment Button -->
                                                <StackPanel Grid.Row="0" Grid.Column="1" Orientation="Horizontal"  HorizontalAlignment="Stretch" VerticalAlignment="Center" Height="50"  Margin="10,0,10,0">
                                                    <Label Content="Itinerary for" HorizontalAlignment="Left" FontWeight="Regular" Margin="0,0,0,0" VerticalAlignment="Center" FontFamily="Calibri" Width="auto" Padding="0,0,5,0"  />
                                                    <ComboBox x:Name="CmbTourlist" FontFamily="Calibri" Width="auto" Height="24" FontSize="14" VerticalContentAlignment="Center" Padding="5,0,5,0" BorderBrush="#FFA1A1A1">
                                                        <ComboBox.Background>
                                                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0" MappingMode="RelativeToBoundingBox">
                                                                <GradientStop Color="White"/>
                                                                <GradientStop Color="White" Offset="1"/>
                                                            </LinearGradientBrush>
                                                        </ComboBox.Background>
